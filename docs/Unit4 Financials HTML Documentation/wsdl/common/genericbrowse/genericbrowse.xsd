<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:com="http://www.coda.com/efinance/schemas/common" xmlns:gmb="http://www.coda.com/common/schemas/genericbrowsemaster" xmlns:gnb="http://www.coda.com/common/schemas/genericbrowse" xmlns:sel="http://www.coda.com/efinance/schemas/selector" xmlns:pre="http://www.coda.com/efinance/schemas/presenter" xmlns:selmas="http://www.coda.com/efinance/schemas/selectormaster" xmlns:build="http://www.coda.com/common/schemas/funcsec" targetNamespace="http://www.coda.com/common/schemas/genericbrowse" elementFormDefault="qualified"><xsd:import namespace="http://www.coda.com/efinance/schemas/selector" schemaLocation="../../../wsdl/finance/selector/selector.xsd"/><xsd:import namespace="http://www.coda.com/common/schemas/genericbrowsemaster" schemaLocation="../../../wsdl/common/genericbrowsemaster/genericbrowsemaster.xsd"/><xsd:import namespace="http://www.coda.com/efinance/schemas/selectormaster" schemaLocation="../../../wsdl/finance/selectormaster/selectormaster.xsd"/><xsd:import namespace="http://www.coda.com/efinance/schemas/common" schemaLocation="../../../wsdl/finance/common/common.xsd"/><xsd:import namespace="http://www.coda.com/efinance/schemas/presenter" schemaLocation="../../../wsdl/finance/presenter/presenter.xsd"/>   <xsd:complexType name="VocabIdList">     <xsd:annotation>       <xsd:documentation>This element contains a list of vocabulary items.</xsd:documentation>     </xsd:annotation>   <xsd:sequence>    <xsd:element maxOccurs="unbounded" minOccurs="0" name="VocabId" type="xsd:string">         <xsd:annotation>           <xsd:documentation>This element contains a vocabulary item.</xsd:documentation>         </xsd:annotation>       </xsd:element>   </xsd:sequence>  </xsd:complexType>                               <xsd:complexType name="SelectRequest">   <xsd:complexContent>    <xsd:extension base="com:Request">     <xsd:sequence>      <xsd:element name="PresenterCode" type="com:typeGeneralCode">             <xsd:annotation>               <xsd:documentation>The code for the presenter master to be used to display the data.</xsd:documentation>             </xsd:annotation>           </xsd:element>      <xsd:element name="SecurityCode" type="com:typeGeneralCode">             <xsd:annotation>               <xsd:documentation>The code for the data security master to be used to control data security.</xsd:documentation>             </xsd:annotation>           </xsd:element>      <xsd:element name="RunTimeSelector" type="selmas:SelectorMaster">             <xsd:annotation>               <xsd:documentation>Contains the selector master to be used to retrieve data, including any run-time changes the user made and any values the user entered in response to prompts.</xsd:documentation>             </xsd:annotation>           </xsd:element>      <xsd:element minOccurs="0" name="ExtraVocabs" type="gnb:VocabIdList">             <xsd:annotation>               <xsd:documentation>A list of extra vocabulary items to return in the presentation results.</xsd:documentation>             </xsd:annotation>           </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>  <xsd:complexType name="SelectRequestVerb">     <xsd:annotation>       <xsd:documentation>Retrieves data from the database using the specified presenter and selector information.</xsd:documentation>     </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:RequestVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="gnb:SelectRequest"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>  <xsd:complexType name="SelectResponse">   <xsd:complexContent>    <xsd:extension base="com:Response">     <xsd:sequence>      <xsd:element name="Selection" type="sel:Selection">             <xsd:annotation>               <xsd:documentation>This element holds your data.</xsd:documentation>             </xsd:annotation>           </xsd:element>      <xsd:element name="PrimaryKeyVocabs" type="pre:VocabList">       <xsd:annotation>        <xsd:documentation>         <p>          Contains vocabs detailing the primary key of the          primary object (table) of the dataview that we          are browsing. This is for the benefit of generic          printing.                    N.B. This limits generic printing to only being able          to print (or not print) documents based on the primary          key of the dataview. e.g. if your presenter is showing          you more than one line with the same primary key, you           will not be able to print one but-not-the-others of these          lines on an individual basis. You will only be able to           print one &apos;document&apos; containing all the line with          the same primary key. Lines with other primary keys will          appear in other &apos;documents&apos;.                    To get round this limitation, create a new dataview with          a primary key that suits your needs.         </p>        </xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>  <xsd:complexType name="SelectResponseVerb">     <xsd:annotation>       <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>     </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:ResponseVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="gnb:SelectResponse"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="PrintRequest">   <xsd:complexContent>    <xsd:extension base="com:Request">        <xsd:sequence>                <xsd:element name="DataSecurityMasterCode" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The code for the data security master that controls the data security that applies to the user. This is required to ensure that the user is not able to print or transmit lines that are filtered out when browsing.</xsd:documentation>       </xsd:annotation>      </xsd:element>                 <xsd:element name="RunTimeSelector" type="selmas:SelectorMaster">       <xsd:annotation>        <xsd:documentation>Contains the selector master that was used to retrieve data, including any run-time changes the user made and any values the user entered in response to prompts. (This improves performance when finding the lines to print.)</xsd:documentation>       </xsd:annotation>      </xsd:element>           <xsd:element name="OutputDeviceCode" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The output device to use to print or transmit rows.</xsd:documentation>       </xsd:annotation>      </xsd:element>            <xsd:element name="PrintFormatCode" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>                    Code of the print format to be used. The print format should ..                    a) if tables are being used, have its form type set to &apos;flow&apos;.              b) if the order of printing is important, have a sort order on it.        </xsd:documentation>       </xsd:annotation>      </xsd:element>            <xsd:element name="EmailTemplateCode" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The code of an e-mail template master used to provide the subject, body and signature of generated emails.</xsd:documentation>       </xsd:annotation>      </xsd:element>            <xsd:element name="ChosenRows" type="sel:Selection">       <xsd:annotation>        <xsd:documentation>         <p>          A selection containing the primary key cells          of the rows that need to be printed. These should           correspond to the primary key of the primary object          of the dataview that this generic browse/print is          based on.                    N.B. Printing only has the granularity to print dataview          lines based on the primary key of the primary object          of the dataview. Therefore if your generic browse           screen displays two lines that both have the same           primary key, it is not possible to print one without           the other one also being printed. To get round this,          define a new dataview that has a more suitable primary          key.            </p>        </xsd:documentation>       </xsd:annotation>      </xsd:element>            <xsd:element name="DoLogging" type="xsd:boolean">       <xsd:annotation>        <xsd:documentation>Specifies whether to write a log file. If this element is set to true, then the elements LogType and LogTitle must also be set.</xsd:documentation>       </xsd:annotation>      </xsd:element>       <xsd:element name="LogTitle" type="xsd:string">       <xsd:annotation>        <xsd:documentation>The title for the log file. This needs to be set only if the DoLogging element is set to true.</xsd:documentation>       </xsd:annotation>      </xsd:element>           <xsd:element name="LogType" type="com:typeGenericOon">       <xsd:annotation>        <xsd:documentation>The type of the log. This needs to be set only if the DoLogging element is set to true.</xsd:documentation>       </xsd:annotation>      </xsd:element>           </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>  <xsd:complexType name="PrintRequestVerb">     <xsd:annotation>       <xsd:documentation>Sends the rows listed in the ChosenRows element to Print Formatter, where they are formatted in accordance with the specified print format and then printed or transmitted in accordance with the specified output device.</xsd:documentation>     </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:RequestVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="gnb:PrintRequest"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="PrintResponse">   <xsd:complexContent>    <xsd:extension base="com:Response">       </xsd:extension>   </xsd:complexContent>  </xsd:complexType>  <xsd:complexType name="PrintResponseVerb">     <xsd:annotation>       <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>     </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:ResponseVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="gnb:PrintResponse"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>     <xsd:complexType name="SelectForLedgerRequest">   <xsd:complexContent>    <xsd:extension base="com:Request">     <xsd:sequence>      <xsd:element name="LedgerKey" type="com:Key">             <xsd:annotation>               <xsd:documentation>The key to a browse ledger master.</xsd:documentation>             </xsd:annotation>           </xsd:element>      <xsd:element name="RunTimeSelector" type="selmas:SelectorMaster">             <xsd:annotation>               <xsd:documentation>Contains the selector master to be used to retrieve data, including any run-time changes the user made and any values the user entered in response to prompts.</xsd:documentation>             </xsd:annotation>           </xsd:element>      <xsd:element minOccurs="0" name="ExtraVocabs" type="gnb:VocabIdList">             <xsd:annotation>               <xsd:documentation>A list of extra vocabulary items to return in the presentation results.</xsd:documentation>             </xsd:annotation>           </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>  <xsd:complexType name="SelectForLedgerRequestVerb">     <xsd:annotation>       <xsd:documentation>Retrieves data from the database using a specified browse ledger master and selector master.</xsd:documentation>     </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:RequestVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="gnb:SelectForLedgerRequest"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>  <xsd:complexType name="SelectForLedgerResponse">   <xsd:complexContent>    <xsd:extension base="com:Response">     <xsd:sequence>      <xsd:element name="Selection" type="sel:Selection">             <xsd:annotation>               <xsd:documentation>This element holds your data.</xsd:documentation>             </xsd:annotation>           </xsd:element>      <xsd:element name="PrimaryKeyVocabs" type="pre:VocabList">             <xsd:annotation>               <xsd:documentation>A list of the vocabulary items that describe the primary key of the primary object of the dataview.</xsd:documentation>             </xsd:annotation>           </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>  <xsd:complexType name="SelectForLedgerResponseVerb">     <xsd:annotation>       <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>     </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:ResponseVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="gnb:SelectForLedgerResponse"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="SelectForTransactionsRequest">   <xsd:complexContent>    <xsd:extension base="com:Request">     <xsd:sequence>      <xsd:element name="PresenterCode" type="com:typeGeneralCode">             <xsd:annotation>               <xsd:documentation>The code for the presenter master to be used to display the data.</xsd:documentation>             </xsd:annotation>           </xsd:element>      <xsd:element name="RunTimeSelector" type="selmas:SelectorMaster">             <xsd:annotation>               <xsd:documentation>Contains the selector master to be used to retrieve data, including any run-time changes the user made and any values the user entered in response to prompts.</xsd:documentation>             </xsd:annotation>           </xsd:element>      <xsd:element minOccurs="0" name="ExtraVocabs" type="gnb:VocabIdList">             <xsd:annotation>               <xsd:documentation>A list of extra vocabulary items to return in the presentation results.</xsd:documentation>             </xsd:annotation>           </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>  <xsd:complexType name="SelectForTransactionsRequestVerb">     <xsd:annotation>       <xsd:documentation>Retrieves data from the database using a specified presenter master and selector master.</xsd:documentation>     </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:RequestVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="gnb:SelectForTransactionsRequest"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>  <xsd:complexType name="SelectForTransactionsResponse">   <xsd:complexContent>    <xsd:extension base="com:Response">     <xsd:sequence>      <xsd:element name="Selection" type="sel:Selection">             <xsd:annotation>               <xsd:documentation>This element holds your data.</xsd:documentation>             </xsd:annotation>           </xsd:element>      <xsd:element name="PrimaryKeyVocabs" type="pre:VocabList">             <xsd:annotation>               <xsd:documentation>A list of the vocabulary items that describe the primary key of the primary object of the dataview.</xsd:documentation>             </xsd:annotation>           </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>  <xsd:complexType name="SelectForTransactionsResponseVerb">     <xsd:annotation>       <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>     </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:ResponseVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="gnb:SelectForTransactionsResponse"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>       <xsd:complexType name="SelectForLinkMaintenanceRequest">         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element name="PresenterCode" type="com:typeGeneralCode">             <xsd:annotation>               <xsd:documentation>The code for the presenter master to be used to display the data.</xsd:documentation>             </xsd:annotation>           </xsd:element>                     <xsd:element name="RunTimeSelector" type="selmas:SelectorMaster">             <xsd:annotation>               <xsd:documentation>Contains the selector master to be used to retrieve data, including any run-time changes the user made and any values the user entered in response to prompts.</xsd:documentation>             </xsd:annotation>           </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="SelectForLinkMaintenanceRequestVerb">     <xsd:annotation>       <xsd:documentation>Retrieves data from the database using a specified presenter master and selector master.</xsd:documentation>     </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="gnb:SelectForLinkMaintenanceRequest"/>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="SelectForLinkMaintenanceResponse">         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence>                     <xsd:element name="Selection" type="sel:Selection">             <xsd:annotation>               <xsd:documentation>This element holds your data.</xsd:documentation>             </xsd:annotation>           </xsd:element>                     <xsd:element name="PrimaryKeyVocabs" type="gnb:VocabIdList">             <xsd:annotation>               <xsd:documentation>A list of the vocabulary items that describe the primary key of the primary object of the dataview.</xsd:documentation>             </xsd:annotation>           </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="SelectForLinkMaintenanceResponseVerb">     <xsd:annotation>       <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>     </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="gnb:SelectForLinkMaintenanceResponse"/>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>   <xsd:complexType name="GenericBrowseRequest">   <xsd:annotation>    <xsd:appinfo build:funcSecValue="445" build:readOnly="true"/>       <xsd:documentation>This service allows you to retrieve data from the database using metadata selectors and presenters.</xsd:documentation>   </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:ServiceRequest">     <xsd:sequence>      <xsd:element minOccurs="0" name="Select" type="gnb:SelectRequestVerb">       <xsd:annotation>        <xsd:appinfo build:funcSecValue="1" build:readOnly="true"/>               <xsd:documentation>Retrieves data from the database using the specified presenter and selector information.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="Print" type="gnb:PrintRequestVerb">       <xsd:annotation>        <xsd:appinfo build:funcSecValue="2" build:readOnly="true"/>               <xsd:documentation>Sends the rows listed in the ChosenRows element to Print Formatter, where they are formatted in accordance with the specified print format and then printed or transmitted in accordance with the specified output device.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="SelectForLedger" type="gnb:SelectForLedgerRequestVerb">             <xsd:annotation>               <xsd:documentation>Retrieves data from the database using a specified browse ledger master and selector master.</xsd:documentation>             </xsd:annotation>           </xsd:element>      <xsd:element minOccurs="0" name="SelectForTransactions" type="gnb:SelectForTransactionsRequestVerb">             <xsd:annotation>               <xsd:documentation>Retrieves data from the database using a specified presenter master and selector master.</xsd:documentation>             </xsd:annotation>           </xsd:element>                     <xsd:element minOccurs="0" name="SelectForLinkMaintenance" type="gnb:SelectForLinkMaintenanceRequestVerb">             <xsd:annotation>               <xsd:documentation>Retrieves data from the database using a specified presenter master and selector master.</xsd:documentation>             </xsd:annotation>           </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>  <xsd:complexType name="GenericBrowseResponse">     <xsd:annotation>       <xsd:documentation>This is the response that is returned when you submit a generic browse request.</xsd:documentation>     </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:ServiceResponse">     <xsd:sequence>      <xsd:element minOccurs="0" name="Select" type="gnb:SelectResponseVerb">             <xsd:annotation>               <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>             </xsd:annotation>           </xsd:element>      <xsd:element minOccurs="0" name="Print" type="gnb:PrintResponseVerb">             <xsd:annotation>               <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>             </xsd:annotation>           </xsd:element>      <xsd:element minOccurs="0" name="SelectForLedger" type="gnb:SelectForLedgerResponseVerb">             <xsd:annotation>               <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>             </xsd:annotation>           </xsd:element>      <xsd:element minOccurs="0" name="SelectForTransactions" type="gnb:SelectForTransactionsResponseVerb">             <xsd:annotation>               <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>             </xsd:annotation>           </xsd:element>                     <xsd:element minOccurs="0" name="SelectForLinkMaintenance" type="gnb:SelectForLinkMaintenanceResponseVerb">             <xsd:annotation>               <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>             </xsd:annotation>           </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>        <xsd:element name="GenericBrowseRequest" type="gnb:GenericBrowseRequest">     <xsd:annotation>       <xsd:documentation>This service allows you to retrieve data from the database using metadata selectors and presenters.</xsd:documentation>     </xsd:annotation>   </xsd:element>  <xsd:element name="GenericBrowseResponse" type="gnb:GenericBrowseResponse">     <xsd:annotation>       <xsd:documentation>This is the response that is returned when you submit a generic browse request.</xsd:documenta