                     <xsd:documentation>The task is an out                                 argument, so the server will put it here.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="ListTaskInfoResponseVerb">         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="tasks:ListTaskInfoResponse"/>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetTaskInfoRequest">         <xsd:annotation>             <xsd:documentation>GetTaskInfo.<p>Get the metadata for a                     task. </p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element name="Type" type="com:typeCode">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The type of task.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetTaskInfoRequestVerb">         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="tasks:GetTaskInfoRequest"/>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetTaskInfoResponse">         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence>                     <xsd:element name="GetTaskInfo" type="tasks:TaskInfo">                         <xsd:annotation>                             <xsd:documentation>The return value of the                                 getTaskInfo function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetTaskInfoResponseVerb">         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="tasks:GetTaskInfoResponse"/>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="ListTaskTypesRequest">         <xsd:annotation>             <xsd:documentation>ListTaskTypes.<p>List the types of task                     available to be executed by the server.</p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence/>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="ListTaskTypesRequestVerb">         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="tasks:ListTaskTypesRequest"/>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="ListTaskTypesResponse">         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Type" type="com:typeCode">                         <xsd:annotation>                             <xsd:documentation>The type is an out                                 argument, so the server will put it here.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="ListTaskTypesResponseVerb">         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="tasks:ListTaskTypesResponse"/>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="Parameters">                  <xsd:sequence>                    <xsd:element name="ParamCollection" type="tasks:ParamCollection"/>                      <xsd:any/>                 </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="RunRequest">         <xsd:annotation>             <xsd:documentation>Run.<p>Run a new task of the specified                     type and passing the parameters given.</p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element name="Type" type="com:typeCode">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The type of task to run.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element name="Parameters" type="tasks:Parameters"/>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>   