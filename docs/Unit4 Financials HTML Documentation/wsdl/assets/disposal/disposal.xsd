ment name="ExpensesAccount" type="com:typeAcCodeB">     <xsd:annotation>      <xsd:documentation>The account to use for the expenses of the disposal.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ExpensesTransferAccount" type="com:typeAcCodeB">     <xsd:annotation>      <xsd:documentation>The account to use for the expenses of the transfer.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="PartialDisposalValue" type="com:typeMoney">     <xsd:annotation>      <xsd:documentation>The value of the disposal.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="PartialDisposalValueRate" type="com:typeNumber">     <xsd:annotation>      <xsd:documentation>The percentage rate of the disposal.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="PartialDisposalCurrencyList" type="ass:AssetCurrencyDataList">     <xsd:annotation>      <xsd:documentation>A list of values expressed in different currencies.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="PostingProceeds" type="com:typeMoney">     <xsd:annotation>      <xsd:documentation>The value of the proceeds of the disposal.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ProceedsCurrencyList" type="ass:AssetCurrencyDataList">     <xsd:annotation>      <xsd:documentation>A list of values expressed in different currencies.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="PostingExpense" type="com:typeMoney">     <xsd:annotation>      <xsd:documentation>The value of the expenses for the disposal.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ExpenseCurrencyList" type="ass:AssetCurrencyDataList">     <xsd:annotation>      <xsd:documentation>A list of values expressed in different currencies.</xsd:documentation>     </xsd:annotation>    </xsd:element>   </xsd:sequence>  </