annotation>   <xsd:complexContent>    <xsd:extension base="com:RequestVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="popcap:GetRequest"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>  <xsd:complexType name="GetResponse">   <xsd:complexContent>    <xsd:extension base="com:Response">     <xsd:sequence>      <xsd:element name="Capability" type="popcap:POPCapability">       <xsd:annotation>        <xsd:documentation>Contains the capability master you have retrieved from the database.</xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>  <xsd:complexType name="GetResponseVerb">   <xsd:annotation>    <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>   </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:ResponseVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="popcap:GetResponse"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>  <xsd:complexType name="POPCapabilityMasterRequest">   <xsd:annotation>    <xsd:appinfo build:funcSecValue="744" build:readOnly="true"/>    <xsd:documentation>This service allows you to maintain the Procurement settings on capability masters.</xsd:documentation>   </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:ServiceRequest">     <xsd:sequence>      <xsd:element minOccurs="0" name="Get" type="popcap:GetRequestVerb">       <xsd:annotation>        <xsd:appinfo build:funcSecValue="1" build:readOnly="true"/>        <xsd:documentation>Retrieves the specified contract masters from the database.</xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>  <xsd:complexType name="POPCapabilityMasterResponse">   <xsd:annotation>    <xsd:documentation>This service holds the response to your request to maintain the Procurement settings on capability masters.</xsd:documentation>   </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:ServiceResponse">     <xsd:sequence>      <xsd:element minOccurs="0" name="Get" type="popcap:GetResponseVerb">       <xsd:annotation>        <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>    <xsd:element name="POPCapabilityMasterRequest" type="popcap:POPCapabilityMasterRequest">   <xsd:annotation>    <xsd:documentation>This service allows you to maintain the Procurement settings on capability masters.</xsd:documentation>   </xsd:annotation>  </xsd:element>  <xsd:element name="POPCapabilityMasterResponse" type="popcap:POPCapabilityMasterResponse">   <xsd:annotation>    <xsd:documentation>This service holds the response to your request to maintain the Procurement settings on capability masters.</xsd:documentation>   </xsd:annotation>  </xsd:element>               <xsd:complexType name="POPCapability">   <xsd:annotation>    <xsd:documentation>This element holds details of the Procurement settings on the capability master.</xsd:documentation>   </xsd:annotation>   <xsd:sequence>    <xsd:element name="ProcessMasterList" type="popusr:ProcessMasterInfoList">     <xsd:annotation>      <xsd:documentation>The list of process masters that this capability has access to.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ProductGroupSecurityEnabled" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies whether product group security is enabled.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ProductGroupList" type="mas:MasterKeyList">     <xsd:annotation>      <xsd:documentation>The list of product groups that this capability has access to.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="BudgetList" type="mas:MasterKeyList">     <xsd:annotation>      <xsd:documentation>This element is not used.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ChangeTax" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can change the tax code used by documents.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ChangeCurrency" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can change document currency.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ChangeTerms" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can change supplier terms on purchase orders.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ChangeDeliveryDate" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can change system-generated delivery dates.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ChangeDeliveryDatePostIssue" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can change system-generated delivery dates after the document has been authorised.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ChangeAuthorisingUser" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can change the authorising user when submitting a document for authorisation.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="PrintUnauthorisedDocs" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can print unauthorised documents.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ModifyKit" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>This element is not used.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="OverrideTransactionLimit" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can override the transaction limit held on the supplier element.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="AllowPartialCompletion" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can partially complete order lines regardless of whether a receipt has been matched to them.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ModifyRequistionPrice" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can change the price of an item on a requisition.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="AmendSupplierName" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can change the name line of a supplier address.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="AmendSupplierAddress" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can change the whole of a supplier address.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="AmendDeliveryDefault" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can choose a different address from the list of delivery addresses.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="AmendTopLine" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can change the top line of a delivery address.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="AmendAllLines" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can change the whole of a delivery address.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ReceiveAllLines" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>This element is not used.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="EditAccountSplits" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can edit the received quantity/value on split lines.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="CommitValueVariance" type="xsd:string">     <xsd:annotation>      <xsd:documentation>The value by which this capability can exceed the budget.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="CommitPercentageVariance" type="xsd:string">     <xsd:annotation>      <xsd:documentation>The percentage of the available budget by which this capability can exceed it.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ToleranceCurrencyCode" type="com:typeCode">     <xsd:annotation>      <xsd:documentation>The currency code in which line completion tolerances are expressed.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="UnderValueVarianceValue" type="Poptypes:typePopToleranceValue">     <xsd:annotation>      <xsd:documentation>For under-deliveries, the fixed monetary value less than the order value within which this capability can complete the order line.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="UnderValueVariancePercentage" type="Poptypes:typePopUnderTolerancePercentage">     <xsd:annotation>      <xsd:documentation>For under-deliveries, the percentage value less than the order value within which this capability can complete the order line.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="UnderQuantityVarianceValue" type="Poptypes:typePopToleranceValue">     <xsd:annotation>      <xsd:documentation>For under-deliveries, the fixed amount less than the order quantity within which this capability can complete the order line.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="UnderQuantityVariancePercentage" type="Poptypes:typePopUnderTolerancePercentage">     <xsd:annotation>      <xsd:documentation>For under-deliveries, the percentage quantity less than the order quantity within which this capability can complete the order line.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="OverValueVarianceValue" type="Poptypes:typePopToleranceValue">     <xsd:annotation>      <xsd:documentation>For over-deliveries, the fixed monetary value more than the order value within which this capability can complete the order line.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="OverValueVariancePercentage" type="Poptypes:typePopOverTolerancePercentage">     <xsd:annotation>      <xsd:documentation>For over-deliveries, the percentage value more than the order value within which this capability can complete the order line.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="OverQuantityVarianceValue" type="Poptypes:typePopToleranceValue">     <xsd:annotation>      <xsd:documentation>For over-deliveries, the fixed amount more than the order quantity within which this capability can complete the order line.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="OverQuantityVariancePercentage" type="Poptypes:typePopOverTolerancePercentage">     <xsd:annotation>      <xsd:documentation>For over-deliveries, the percentage quantity more than the order quantity within which this capability can complete the order line.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="OverrideHeaderAccountRules" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can override summary line account codes.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="OverrideLineAccountRules" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can override item line account codes.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="ChangeRate" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>This element is not used.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="OverrideAccountRules" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>This element is not used.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="AllowAllAuthorisingUsers" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>This element is not used.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="AllowResetToDraft" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies that this capability can reset the status of protected documents to Draft.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="AllowModifyReceivedOrders" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Specifies whether users are allowed to make changes to orders with received lines.</xsd:documentation>     </xsd:annotation>    </xsd:element>   </xsd:sequence>  </xsd:complexType> </xsd:schema>