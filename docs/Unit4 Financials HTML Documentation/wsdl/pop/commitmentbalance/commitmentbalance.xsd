<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:poptypes="http://www.coda.com/pop/schemas/common" xmlns:com="http://www.coda.com/efinance/schemas/common" xmlns:commbalance="http://www.coda.com/pop/schemas/commitmentbalance" targetNamespace="http://www.coda.com/pop/schemas/commitmentbalance" elementFormDefault="qualified"><xsd:import namespace="http://www.coda.com/efinance/schemas/common" schemaLocation="../../../wsdl/finance/common/common.xsd"/><xsd:import namespace="http://www.coda.com/pop/schemas/common" schemaLocation="../../../wsdl/pop/common/common.xsd"/>                                   <xsd:complexType name="CommitmentBudget">         <xsd:annotation>             <xsd:documentation>                 <p>     The budget balances. Used in commitment accounting.    </p>             </xsd:documentation>         </xsd:annotation>         <xsd:sequence>                          <xsd:element maxOccurs="1" minOccurs="1" name="BudgetRejected" type="com:typeBoolean">                 <xsd:annotation>                     <xsd:documentation/>                 </xsd:annotation>             </xsd:element>                          <xsd:element name="BudgetList" type="commbalance:CommitmentBudgetBudgetList">                 <xsd:annotation>                     <xsd:documentation/>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="CommitmentBudgetBudgetList">         <xsd:annotation>             <xsd:documentation>A list of CommitmentBudgets.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="Budget" type="commbalance:ComBudgetInfo">                 <xsd:annotation>                     <xsd:documentation>A single ComBudgetInfo item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="CommitmentBudgetList">         <xsd:annotation>             <xsd:documentation>A list of CommitmentBudgets.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="CommitmentBudget" type="commbalance:CommitmentBudget">                 <xsd:annotation>                     <xsd:documentation>A single CommitmentBudget item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>          <xsd:complexType name="ComBudgetInfo">         <xsd:annotation>             <xsd:documentation>                 <p>This element holds budget information for commitments.</p>             </xsd:documentation>         </xsd:annotation>         <xsd:sequence>                          <xsd:element maxOccurs="1" minOccurs="1" name="CompanyCode" type="com:typeCode">                 <xsd:annotation>                     <xsd:documentation>                         <p>The code of the company in which the balance is checked.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="1" name="BalCode" type="com:typeCode">                 <xsd:annotation>                     <xsd:documentation>                         <p>The budget balance code.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="1" name="AccountCode" type="poptypes:typePopAccountCodeSubstb">                 <xsd:annotation>                     <xsd:documentation>                         <p>      The account that is being updated for this item line.     </p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="1" name="RepBasis" type="com:typeReportingBasis">                 <xsd:annotation>                     <xsd:documentation>                         <p>The intersect that relates to the account on this line.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="1" name="BalYear" type="com:typeHardYearOnly">                 <xsd:annotation>                     <xsd:documentation>                         <p>The year of the budget balance.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="1" name="FreeBudget" type="com:typeNumber">                 <xsd:annotation>                     <xsd:documentation>                         <p>The amount of the specified budget remaining before accounting for this line.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="1" name="Commitment" type="com:typeNumber">                 <xsd:annotation>                     <xsd:documentation>                         <p>The expected commitment value for this line, in home currency.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="1" name="PostCheckBudget" type="com:typeNumber">                 <xsd:annotation>                     <xsd:documentation>                         <p>The amount remaining after accounting for this line.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="1" name="BudgetStatus" type="com:typeGenericOon">                 <xsd:annotation>                     <xsd:documentation>                         <p>The expected outcome of the budget check when this line is processed.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="1" name="ToleranceUsed" type="com:typeBoolean">                 <xsd:annotation>                     <xsd:documentation>                         <p>Specifies whether budget tolerance is used, as specified on the capability master.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="ComBudgetInfoList">         <xsd:annotation>             <xsd:documentation>A list of ComBudgetInfos.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="ComBudgetInfo" type="commbalance:ComBudgetInfo">                 <xsd:annotation>                     <xsd:documentation>A single ComBudgetInfo item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>      </xsd:schema>