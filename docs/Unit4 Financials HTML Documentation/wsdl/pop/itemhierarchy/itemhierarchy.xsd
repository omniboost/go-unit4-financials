<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:poptypes="http://www.coda.com/pop/schemas/common" xmlns:elmmas="http://www.coda.com/pop/schemas/elementmaster" xmlns:com="http://www.coda.com/efinance/schemas/common" xmlns:docmas="http://www.coda.com/pop/schemas/documentmaster" xmlns:itemhierarchy="http://www.coda.com/pop/schemas/itemhierarchy" xmlns:popprdgrpmas="http://www.coda.com/pop/schemas/productgroupmaster" xmlns:build="http://www.coda.com/common/schemas/funcsec" xmlns:popitem="http://www.coda.com/pop/schemas/item" targetNamespace="http://www.coda.com/pop/schemas/itemhierarchy" elementFormDefault="qualified"><xsd:import namespace="http://www.coda.com/pop/schemas/productgroupmaster" schemaLocation="../../../wsdl/pop/productgroupmaster/productgroupmaster.xsd"/><xsd:import namespace="http://www.coda.com/pop/schemas/item" schemaLocation="../../../wsdl/pop/item/item.xsd"/><xsd:import namespace="http://www.coda.com/pop/schemas/elementmaster" schemaLocation="../../../wsdl/pop/elementmaster/elementmaster.xsd"/><xsd:import namespace="http://www.coda.com/pop/schemas/documentmaster" schemaLocation="../../../wsdl/pop/documentmaster/documentmaster.xsd"/><xsd:import namespace="http://www.coda.com/efinance/schemas/common" schemaLocation="../../../wsdl/finance/common/common.xsd"/><xsd:import namespace="http://www.coda.com/pop/schemas/common" schemaLocation="../../../wsdl/pop/common/common.xsd"/>                                                       <xsd:annotation>         <xsd:documentation>Schema for the POPItemHierarchy service.<p>      The POPItemHierarchy service exposes functions for altering and querying the Item Hierarchy.     </p>         </xsd:documentation>     </xsd:annotation>     <xsd:complexType name="BatchUpdateRequest">         <xsd:annotation>             <xsd:documentation>BatchUpdate.<p>       Adds a reference to an Item to the hierarchy. Returns a collection representing the new child and its new siblings.      </p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element name="BatchList" type="itemhierarchy:POPItemHierarchyBatchUpdateBatchList">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        Key of the Item to add to the hierarchy.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="POPItemHierarchyBatchUpdateBatchList">         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="Batch" type="itemhierarchy:ItemHierarchyBatchEntry">                 <xsd:annotation>                     <xsd:documentation>As the batch argument is a collection argument, this type is used to hold the instances of the batch argument.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="BatchUpdateRequestVerb">         <xsd:annotation>             <xsd:documentation>The request of the batchUpdate function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="itemhierarchy:BatchUpdateRequest">                         <xsd:annotation>                             <xsd:documentation>The details of the request for the batchUpdate function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="BatchUpdateResponse">         <xsd:annotation>             <xsd:documentation>The response of the batchUpdate function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Batch" type="itemhierarchy:ItemHierarchyBatchEntry">                         <xsd:annotation>                             <xsd:documentation>The batch is an out argument, so the server will put it here.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="BatchUpdateResponseVerb">         <xsd:annotation>             <xsd:documentation>The response verb of the batchUpdate function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="itemhierarchy:BatchUpdateResponse">                         <xsd:annotation>                             <xsd:documentation>The details of the response for the batchUpdate function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="AddItemRequest">         <xsd:annotation>             <xsd:documentation>AddItem.<p>       Adds a reference to an Item to the hierarchy. Returns a collection representing the new child and its new siblings.      </p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element maxOccurs="1" minOccurs="1" name="Item" type="popitem:ItemKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        Key of the Item to add to the hierarchy.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="0" name="ParentProductGroupMaster" type="popprdgrpmas:ProductGroupMasterKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        Key of the Product Group Master which will adopt the child. If this is null then the Item is added at the top level in the hierarchy.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="1" name="Position" type="com:typeULong">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        The position of the new Item in relation to its siblings. If zero (or negative), then this item will head the list. If greater than the number of items in the list, it will be        appended to the end (and given a new appropriate number). If the specified position is already occupied then it will be inserted at that position.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="AddItemRequestVerb">         <xsd:annotation>             <xsd:documentation>The request of the addItem function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="itemhierarchy:AddItemRequest">                         <xsd:annotation>                             <xsd:documentation>The details of the request for the addItem function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="AddItemResponse">         <xsd:annotation>             <xsd:documentation>The response of the addItem function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence>                     <xsd:element name="AddItem" type="itemhierarchy:ItemEntryList">                         <xsd:annotation>                             <xsd:documentation>The return value of the addItem function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="AddItemResponseVerb">         <xsd:annotation>             <xsd:documentation>The response verb of the addItem function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="itemhierarchy:AddItemResponse">                         <xsd:annotation>                             <xsd:documentation>The details of the response for the addItem function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="DeleteItemRequest">         <xsd:annotation>             <xsd:documentation>DeleteItem.<p>       Removes a reference to an Item from the hierarchy.      </p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element maxOccurs="1" minOccurs="0" name="HierarchyEntry" type="itemhierarchy:ItemHierarchyRelationshipKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        Key of the ItemHierarchyRelationship to delete. This relationship must represent an Item child.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="DeleteItemRequestVerb">         <xsd:annotation>             <xsd:documentation>The request of the deleteItem function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="itemhierarchy:DeleteItemRequest">                         <xsd:annotation>                             <xsd:documentation>The details of the request for the deleteItem function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="DeleteItemResponse">         <xsd:annotation>             <xsd:documentation>The response of the deleteItem function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence>                     <xsd:element name="DeleteItem" type="itemhierarchy:ItemEntryList">                         <xsd:annotation>                             <xsd:documentation>The return value of the deleteItem function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="DeleteItemResponseVerb">         <xsd:annotation>             <xsd:documentation>The response verb of the deleteItem function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="itemhierarchy:DeleteItemResponse">                         <xsd:annotation>                             <xsd:documentation>The details of the response for the deleteItem function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetChildrenRequest">         <xsd:annotation>             <xsd:documentation>GetChildren.<p>       Returns a list of children of the given parent. If &apos;parentHierarchyEntry&apos; and &apos;parentProductGroupMaster&apos; are both null then all the top       level entries in the hierarchy are returned.      </p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element maxOccurs="1" minOccurs="0" name="ParentHierarchyEntry" type="itemhierarchy:ItemHierarchyRelationshipKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        The entry in the hierarchy whose children are being looked for.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="0" name="CmpCode" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        Optional parameter restricts the search to the given company.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="0" name="ParentProductGroupMaster" type="popprdgrpmas:ProductGroupMasterKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        Key of the Product Group Master parent.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="1" name="ReturnInactiveEntries" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        Whether or not to include inactive hierarchy entries in the resultset.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="0" name="DocumentMaster" type="docmas:DocumentMasterKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Required parameter to restrict results to what this document master allows</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetChildrenRequestVerb">         <xsd:annotation>             <xsd:documentation>The request of the getChildren function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="itemhierarchy:GetChildrenRequest">                         <xsd:annotation>                             <xsd:documentation>The details of the request for the getChildren function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetChildrenResponse">         <xsd:annotation>             <xsd:documentation>The response of the getChildren function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence>                     <xsd:element name="GetChildren" type="itemhierarchy:ItemEntryList">                         <xsd:annotation>                             <xsd:documentation>The return value of the getChildren function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetChildrenResponseVerb">         <xsd:annotation>             <xsd:documentation>The response verb of the getChildren function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="itemhierarchy:GetChildrenResponse">                         <xsd:annotation>                             <xsd:documentation>The details of the response for the getChildren function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetFilteredChildrenRequest">         <xsd:annotation>             <xsd:documentation>GetFilteredChildren.<p>       Returns a list of children of the given parent. If &apos;parentHierarchyEntry&apos; and &apos;parentProductGroupMaster&apos; are both null then all the top       level entries in the hierarchy are returned.      </p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element maxOccurs="1" minOccurs="0" name="ParentHierarchyEntry" type="itemhierarchy:ItemHierarchyRelationshipKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        The entry in the hierarchy whose children are being looked for.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="0" name="CmpCode" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        Optional parameter restricts the search to the given company.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="0" name="ParentProductGroupMaster" type="popprdgrpmas:ProductGroupMasterKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        Key of the Product Group Master parent.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="1" name="ReturnInactiveEntries" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        Whether or not to include inactive hierarchy entries in the resultset.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="1" name="IgnoreSupplierOverride" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        If true only bring back those items where supplier is in item&apos;s supplier list, ignoring flag on item that allows listed suppliers to be overridden.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="0" name="DocumentMaster" type="docmas:DocumentMasterKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Required parameter to restrict results to what this document master allows</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="0" name="Restriction" type="poptypes:typePopRestrictions">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Required parameter to restrict results to what this document master allows</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="0" name="Supplier" type="elmmas:ElementMasterKey">                         <xsd:annotation>                             <xsd:documentation/>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetFilteredChildrenRequestVerb">         <xsd:annotation>             <xsd:documentation>The request of the getFilteredChildren function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="itemhierarchy:GetFilteredChildrenRequest">                         <xsd:annotation>                             <xsd:documentation>The details of the request for the getFilteredChildren function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetFilteredChildrenResponse">         <xsd:annotation>             <xsd:documentation>The response of the getFilteredChildren function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence>                     <xsd:element name="GetFilteredChildren" type="itemhierarchy:ItemEntryList">                         <xsd:annotation>                             <xsd:documentation>The return value of the getFilteredChildren function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetFilteredChildrenResponseVerb">         <xsd:annotation>             <xsd:documentation>The response verb of the getFilteredChildren function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="itemhierarchy:GetFilteredChildrenResponse">                         <xsd:annotation>                             <xsd:documentation>The details of the response for the getFilteredChildren function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetParentsRequest">         <xsd:annotation>             <xsd:documentation>GetParents.<p>       Returns the parents of the given entry in the Hierarchy. The child can be specified either by a Relationship key, an Item Master key       or a Product Group Master key. A ReasonException is thrown if the given Item master is not present in the hierarchy. If null is returned,       then the given entry appears in the root.      </p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element maxOccurs="1" minOccurs="0" name="HierarchyEntry" type="itemhierarchy:ItemHierarchyRelationshipKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        The entry in the hierarchy whose parent is being looked for.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="0" name="Item" type="popitem:ItemKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        The entry in the hierarchy whose parent is being looked for.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="0" name="ProductGroupMaster" type="popprdgrpmas:ProductGroupMasterKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        The entry in the hierarchy whose parent is being looked for.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="1" name="ReturnInactiveEntries" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        Whether or not to include inactive hierarchy entries in the resultset.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetParentsRequestVerb">         <xsd:annotation>             <xsd:documentation>The request of the getParents function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="itemhierarchy:GetParentsRequest">                         <xsd:annotation>                             <xsd:documentation>The details of the request for the getParents function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetParentsResponse">         <xsd:annotation>             <xsd:documentation>The response of the getParents function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence>                     <xsd:element name="GetParents" type="itemhierarchy:ItemEntryList">                         <xsd:annotation>                             <xsd:documentation>The return value of the getParents function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetParentsResponseVerb">         <xsd:annotation>             <xsd:documentation>The response verb of the getParents function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="itemhierarchy:GetParentsResponse">                         <xsd:annotation>                             <xsd:documentation>The details of the response for the getParents function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="MoveRequest">         <xsd:annotation>             <xsd:documentation>Move.<p>       Moves a child from one point in the hierarchy to another. Returns a collection representing this child and its new siblings.      </p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element maxOccurs="1" minOccurs="1" name="HierarchyEntry" type="itemhierarchy:ItemHierarchyRelationshipKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        The entry in the hierarchy to move.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="0" name="ParentRelationship" type="itemhierarchy:ItemHierarchyRelationshipKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        Key of the ItemHierarchyRelationship which will adopt the child. This relationship must represent a ProductGroupMaster child. If this and the        &apos;parentProductGroupMaster&apos; is null then the Item is added at the top level in the hierarchy. This takes precedence over &apos;parentProductGroupMaster&apos;        if values for both are supplied.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="0" name="ParentProductGroupMaster" type="popprdgrpmas:ProductGroupMasterKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        Key of the Product Group Master which will adopt the child. If this and the &apos;parentRelationship&apos; is null then Item is added at the top level in        the hierarchy.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="0" name="Position" type="com:typeULongB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        The position of the entry in relation to its siblings. If zero (or negative), then this item will head the list. If null, it will be appended to the end (and        given an appropriate number). If the specified position is already occupied then it will be inserted at that position.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="MoveRequestVerb">         <xsd:annotation>             <xsd:documentation>The request of the move function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="itemhierarchy:MoveRequest">                         <xsd:annotation>                             <xsd:documentation>The details of the request for the move function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="MoveResponse">         <xsd:annotation>             <xsd:documentation>The response of the move function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence>                     <xsd:element name="Move" type="itemhierarchy:ItemEntryList">                         <xsd:annotation>                             <xsd:documentation>The return value of the move function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="MoveResponseVerb">         <xsd:annotation>             <xsd:documentation>The response verb of the move function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="itemhierarchy:MoveResponse">                         <xsd:annotation>                             <xsd:documentation>The details of the response for the move function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="SetActiveRequest">         <xsd:annotation>             <xsd:documentation>SetActive.<p>       Sets the child refered to by this relationship to be either active or inactive. The relationship must refer to a Product Group Master child.      </p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element maxOccurs="1" minOccurs="1" name="HierarchyEntry" type="itemhierarchy:ItemHierarchyRelationshipKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        The entry in the hierarchy to set active or inactive.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element maxOccurs="1" minOccurs="1" name="Active" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>        New state of the &apos;active&apos; flag.       </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="SetActiveRequestVerb">         <xsd:annotation>             <xsd:documentation>The request of the setActive function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="itemhierarchy:SetActiveRequest">                         <xsd:annotation>                             <xsd:documentation>The details of the request for the setActive function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="SetActiveResponse">         <xsd:annotation>             <xsd:documentation>The response of the setActive function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence>                     <xsd:element name="SetActive" type="itemhierarchy:ItemHierarchyRelationship">                         <xsd:annotation>                             <xsd:documentation>The return value of the setActive function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd: