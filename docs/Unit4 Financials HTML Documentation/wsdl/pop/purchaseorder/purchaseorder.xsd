/pop/currency/currency.xsd"/><xsd:import namespace="http://www.coda.com/pop/schemas/accountmask" schemaLocation="../../../wsdl/pop/accountmask/accountmask.xsd"/><xsd:import namespace="http://www.coda.com/efinance/schemas/common" schemaLocation="../../../wsdl/finance/common/common.xsd"/><xsd:import namespace="http://www.coda.com/pop/schemas/requestforquote" schemaLocation="../../../wsdl/pop/requestforquote/requestforquote.xsd"/><xsd:import namespace="http://www.coda.com/pop/schemas/inputtemplatemaster" schemaLocation="../../../wsdl/pop/inputtemplatemaster/inputtemplatemaster.xsd"/><xsd:import namespace="http://www.coda.com/pop/schemas/commitmentbalance" schemaLocation="../../../wsdl/pop/commitmentbalance/commitmentbalance.xsd"/><xsd:import namespace="http://www.coda.com/pop/schemas/documentmaster" schemaLocation="../../../wsdl/pop/documentmaster/documentmaster.xsd"/><xsd:import namespace="http://www.coda.com/pop/schemas/elementmaster" schemaLocation="../../../wsdl/pop/elementmaster/elementmaster.xsd"/><xsd:import namespace="http://www.coda.com/pop/schemas/requisition" schemaLocation="../../../wsdl/pop/requisition/requisition.xsd"/>                                                                                                    <xsd:complexType name="PurchaseOrder">         <xsd:annotation>             <xsd:documentation>                 <p>     Purchase Order document.    </p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="doc:DocumentWideCurrencyDocument">                 <xsd:sequence>                                          <xsd:element maxOccurs="1" minOccurs="1" name="SnapshotVersion" type="com:typeUWord">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The document version number.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AuthorisingUser" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The user who either has or must approve this document (depending on AuthorisationStatus)</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="AuthorisationLevel" type="poptypes:typePopAuthorisationLevel">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Authorisation Level, document, line or both.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AccountCodeElement1" type="poptypes:typePopElmTextb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Part of the account code     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AccountCodeElement2" type="poptypes:typePopElmTextb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Part of the account code     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AccountCodeElement3" type="poptypes:typePopElmTextb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Part of the account code     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AccountCodeElement4" type="poptypes:typePopElmTextb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Part of the account code     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AccountCodeElement5" type="poptypes:typePopElmTextb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Part of the account code     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AccountCodeElement6" type="poptypes:typePopElmTextb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Part of the account code     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AccountCodeElement7" type="poptypes:typePopElmTextb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Part of the account code     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AccountCodeElement8" type="poptypes:typePopElmTextb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Part of the account code     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="InterCompanyDestination" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>       the intercompany destination, blank if intercompany not allowed      </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AccountCode" type="poptypes:typePopAccTextb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Concatenated version of the 8 account elements.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="MaskType" type="poptypes:typePopAccountMask">                         <xsd:annotation>                             <xsd:documentation>                                 <p>     The type of account mask. Used to display the title of the      account mask.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="MapDest" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>     Destination code for a multi-company account mask.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element name="MappingList" type="doc:DocumentMappingList">                         <xsd:annotation>                             <xsd:documentation>                                 <p>     Property which determines what the mapping the user has selected. Holds the mapping type and the elmcode if applicable     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="OverrideDefaultAccount" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      has the account code been generated or entered     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HdrIntrastatDetails" type="doc:IstatDetails">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Line-level intrastat details.           <b>What is Intrastat?           Intrastat is the name given to the method of collecting information and producing statistics on the movement of goods between the UK and EU Member States.             Please note: Intrastat should not be confused with EC Sales Lists which are used to collect information on all sales from UK VAT registered traders to VAT registered traders in other EU Member States.     </b>                                 </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="RequiredDate" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Date when order is to be delivered.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element name="ItemLineList" type="poppurchaseorder:PurchaseOrderItemLineList">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      The Item lines associated with the this document.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="ReceivingStatus" type="poptypes:typePopOrderReceivable">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Whether the purchase order is a receivable or non-receivable document.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="GRNStatus" type="poptypes:typePopGrnStatus">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Whether the purchase order is NotReceived, PartReceived, FullyReceived or ReceiptNotRequired.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="CalloffStatus" type="poptypes:typePopCalloffStatus">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Only applies to blanket orders. If set to complete, no more calloffs can be generated against the line.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="MatchStatus" type="poptypes:typePopCommitOrderStatus">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Whether the purchase order is fully, partially or not matched.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="InvoiceStatus" type="poptypes:typePopCommitOrderStatus">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Whether the purchase order is fully, partially or not invoiced.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="BlanketOrder" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Indicates blanket order.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ContractNumber" type="com:typeDescription">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Allows the addition of any reference pertinent to the order      </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AuthorisationWorkflowId" type="com:typeDbGuid">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The id of the worflow started when submitted for approval</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DepartmentDescription" type="com:typeNameB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Department description     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AuthoriserDescription" type="com:typeNameB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Authoriser description     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="SupplierTerms" type="com:typeTermB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Supplier terms taken from the element master     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element name="WorkflowHistoryList" type="doc:WorkflowHistoryList">                         <xsd:annotation>                             <xsd:documentation>                                 <p>History of workflows</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="RaisedOnBehalfOfUser" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Who the document was raised on behalf of.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="WorkflowComment" type="com:typeTextB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The comment added and updated in workflow.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="PurchaseOrderList">         <xsd:annotation>             <xsd:documentation>A list of PurchaseOrders.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="PurchaseOrder" type="poppurchaseorder:PurchaseOrder">                 <xsd:annotation>                     <xsd:documentation>A single PurchaseOrder item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="PurchaseOrderKeyList">         <xsd:annotation>             <xsd:documentation>A list of keys for PurchaseOrder items.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="PurchaseOrder" type="poppurchaseorder:PurchaseOrderKey">                 <xsd:annotation>                     <xsd:documentation>A single key for a PurchaseOrder item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="PurchaseOrderKey">         <xsd:annotation>             <xsd:documentation>A single key for a PurchaseOrder item.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="doc:DocumentWideCurrencyDocumentKey">                 <xsd:sequence/>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>          <xsd:complexType name="Asset">         <xsd:annotation>             <xsd:documentation>                 <p>     Contains details of an asset    </p>             </xsd:documentation>         </xsd:annotation>         <xsd:sequence>                          <xsd:element maxOccurs="1" minOccurs="1" name="Id" type="com:typeDbGuid">                 <xsd:annotation>                     <xsd:documentation>                         <p>      Unique identifier.     </p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="0" name="AssetCategory" type="com:typeCode">                 <xsd:annotation>                     <xsd:documentation>                         <p>      The asset category     </p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="0" name="AssetType" type="poptypes:typePopAssetType">                 <xsd:annotation>                     <xsd:documentation>                         <p>      The asset type, ie. new, child or addition     </p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="0" name="PostAsset" type="poptypes:typePopPostAsset">                 <xsd:annotation>                     <xsd:documentation>                         <p>      Whether the asset is posted to the intray or the register     </p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="0" name="ParentCode" type="com:typeAstCodeB">                 <xsd:annotation>                     <xsd:documentation>                         <p>      Applies only to an asset of type child. Code of parent asset     </p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="0" name="InheritRemainingLife" type="com:typeBoolean">                 <xsd:annotation>                     <xsd:documentation>                         <p>      Applies only to an asset of type child. Determines whether child inherits                                         remaining life of parent.     </p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="0" name="InheritCostCentre" type="com:typeBoolean">                 <xsd:annotation>                     <xsd:documentation>                         <p>      Applies only to an asset of type child. Determines whether child inherits                                         remaining cost centre of parent.     </p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="0" name="InheritLocation" type="com:typeBoolean">                 <xsd:annotation>                     <xsd:documentation>                         <p>      Applies only to an asset of type child. Determines whether child inherits                                         remaining location of parent.     </p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="AssetList">         <xsd:annotation>             <xsd:documentation>A list of Assets.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="Asset" type="poppurchaseorder:Asset">                 <xsd:annotation>                     <xsd:documentation>A single Asset item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="AssetKeyList">         <xsd:annotation>             <xsd:documentation>A list of keys for Asset items.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="Asset" type="poppurchaseorder:AssetKey">                 <xsd:annotation>                     <xsd:documentation>A single key for a Asset item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="AssetKey">         <xsd:annotation>             <xsd:documentation>A single key for a Asset item.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element minOccurs="0" name="Company" type="com:typeCode">                 <xsd:annotation>                     <xsd:documentation>                         <p>      The company master.     </p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>             <xsd:element minOccurs="0" name="DocumentMaster" type="com:typeCode">                 <xsd:annotation>                     <xsd:documentation>                         <p>      The document master.     </p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>             <xsd:element minOccurs="0" name="DocumentNumber" type="poptypes:typePopDocnum">                 <xsd:annotation>                     <xsd:documentation>                         <p>      The document number. This must be blankable so that the server can allocate a number upon creation.     </p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>             <xsd:element minOccurs="0" name="LineNumber" type="com:typeLong">                 <xsd:annotation>                     <xsd:documentation>                         <p>      The line number.     </p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>             <xsd:element minOccurs="0" name="Id" type="com:typeDbGuid">                 <xsd:annotation>                     <xsd:documentation>                         <p>      Unique identifier.     </p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>          <xsd:complexType name="PurchaseOrderItemLine">         <xsd:annotation>             <xsd:documentation>                 <p>Purchase Order extensions to the base DiscountableItemLine definition.</p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="doc:DeliverableItemLine">                 <xsd:sequence>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Asset" type="poppurchaseorder:Asset">                         <xsd:annotation>                             <xsd:documentation>                                 <p>     If the one of the elements in the account code has the asset flag set the      details should be here.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="SetAssetOnLine" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      bound to a checkbox on the asset tab defaults is true     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="KitItem" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Is this item a memeber of a kit     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="GenericItem" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      When set to true, indicates that the item is generic.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AssetItem" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Can be set by the user to indicate that the item should be treated as an asset.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="LeadTime" type="com:typeULong">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Item lead time, from Item Master.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="MinimumOrder" type="com:typeNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Minimum order quantity from Item Master. Optional.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ItemBuyer" type="buy:BuyerKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Buyer for this item if specified on item master, or default buyer      for company.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ContractReference" type="com:typeDescription">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Contract associated with the item, if any.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Approver" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Current approver for this line.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ApplyCA" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Commitment accounting applies to this line.     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="LineStatus" type="poptypes:typePopLnStatus">                         <xsd:annotation>                             <xsd:documentation>                                 <p>     The status of the order line e.g. draft for new order     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AuthorisingUser" type="com:typeCode">                         <xsd:annotation>                             <xsd:documentation>                                 <p>the user who either has or must approve this line (depending on AuthorisationStatus)</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="CompletionStatus" type="poptypes:typePopCompletionStatus">                         <xsd:annotation>                             <xsd:documentation>                                 <p>      Completed or blank     </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name=