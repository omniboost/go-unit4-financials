      <output>
        <soap:header message="trnws:OptionsResponse" part="OptionsResponse" use="literal">
        </soap:header>
        <soap:body use="literal"/>
      </output>
    </operation>
    <operation name="CostCentreDocument">
<documentation></documentation>
      <soap:operation soapAction="uri-coda-webservice/14.000.0011/assets/Transfer/CostCentreDocument" style="document"/>
      <input>
        <soap:header message="trnws:Options" part="Options" use="literal">
        </soap:header>
