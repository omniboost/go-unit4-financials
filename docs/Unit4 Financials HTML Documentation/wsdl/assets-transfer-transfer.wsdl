<documentation></documentation>
      <soap:operation soapAction="uri-coda-webservice/14.000.0011/assets/Transfer/CategoryDocument" style="document"/>
      <input>
        <soap:header message="trnws:Options" part="Options" use="literal">
        </soap:header>
        <soap:body use="literal"/>
