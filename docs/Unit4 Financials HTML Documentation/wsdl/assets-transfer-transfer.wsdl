      <soap:operation soapAction="uri-coda-webservice/14.000.0011/assets/Transfer/AssetDocument" style="document"/>
      <input>
        <soap:header message="trnws:Options" part="Options" use="literal">
        </soap:header>
        <soap:body use="literal"/>
      </input>
      <output>
        <soap:header message="trnws:OptionsResponse" part="OptionsResponse" use="literal">
        </soap:header>
        <soap:body use="literal"/>
      </output>
    </operation>
    <operation name="ValidateCategoryTransfer">
<documentation></documentation>
      <soap:operation soapAction="uri-coda-webservice/14.000.0011/assets/Transfer/ValidateCategoryTransfer" style="document"/>
      <input>
        <soap:header message="trnws:Options" part="Options" use="literal">
