ypeCtAsmJustification">         <xsd:annotation>           <xsd:documentation>The alignment of the text in this column. The available values are Left, Right and Centre.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="Size" type="xsd:short">         <xsd:annotation>           <xsd:documentation>The font size of the text in this column.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="DisplayNewLine" type="xsd:boolean">         <xsd:annotation>           <xsd:documentation>Specifies that this column will be displayed on a new line in the header.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="BrmKey">     <xsd:annotation>       <xsd:documentation>This element contains the key information which identifies the browse report master.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element name="CmpCode" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The code for the company in which the browse report master is being maintained.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="Code" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The code for the browse report master.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="AssetType" type="xsd:boolean">         <xsd:annotation>           <xsd:documentation>Indicates whether this is a Browse Assets report type.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="LedgerType" type="xsd:boolean">         <xsd:annotation>           <xsd:documentation>Indicates whether this is a Browse Ledger report type.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="DetailsType" t