ement>       <xsd:element name="Lines" type="mat:MatchingDataLines">         <xsd:annotation>           <xsd:documentation>This element contains a list of the document lines which can be selected as the driving line or the source for data propagation.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="MatchingDataSources">     <xsd:annotation>       <xsd:documentation>This element contains information about the document lines which can be selected as the driving line or the source for data propagation.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element maxOccurs="unbounded" name="Group" type="mat:MatchingDataGroup">         <xsd:annotation>           <xsd:documentation>This element contains information about matching groups (in the current matching session) that require a driving line or a source for data propagation to be specified.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="UserRefs">     <xsd:annotation>       <xsd:documentation>This element contains user references that will be copied to all document lines in the current matching group.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element minOccurs="0" name="UserRef1" type="com:typeUserRefB">         <xsd:annotation>           <xsd:documentation>The first user reference that will be copied to all document lines in the current matching group.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="UserRef2" type="com:typeUserRefB">         <xsd:annotation>           <xsd:documentation>The second user reference that will be copied to all document lines in the current matching group.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="UserRef3" type="com:typeUserRefB">         <xsd:annotation>           <xsd:documentation>The third user reference that will be copied to all document lines in the current matching group.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="GroupResult">     <xsd:annotation>       <xsd:documentation>This element contains the result of committing or dispersing a specific matching group.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element name="GroupNum" type="xsd:short">         <xsd:annotation>           <xsd:documentation>The number of the matching group.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="CommitType" type="com:typeCtMatchCommitType">         <xsd:annotation>           <xsd:documentation>Indicates whether the match was committed or dispersed.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="Status" type="com:typeCtCommitStat">         <xsd:annotation>           <xsd:documentation>Indicates whether the match was successful.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="MatchCurrCode" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The code of the matching currency for this group.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="RequiresDrivingLine" type="xsd:boolean">         <xsd:annotation>           <xsd:documentation>Indicates whether this matching group requires a driving line to be specified.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="RequiresInputDrivingLine" type="xsd:boolean">         <xsd:annotation>           <xsd:documentation>Indicates whether this matching group requires a driving line to be specified for Matching in Input.</xsd:documentation>         </xsd:annotation>       </xsd:element> 