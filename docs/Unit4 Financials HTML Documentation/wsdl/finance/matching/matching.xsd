ent name="MatchCurrCode" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The code of the matching currency for this document line.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="WriteOffMasterCode" type="com:typeGeneralCodeB">         <xsd:annotation>           <xsd:documentation>The code of a write-off master.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="Currencies" type="mat:Currencies">         <xsd:annotation>           <xsd:documentation>This element contains details of the currencies required by this document line.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="MatchLines">     <xsd:annotation>       <xsd:documentation>This element contains details of the document lines included in this matching group.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element maxOccurs="unbounded" name="Line" type="mat:MatchDetails">         <xsd:annotation>           <xsd:documentation>This element contains details of a document line included in this matching group.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="MatchGroup">     <xsd:annotation>       <xsd:documentation>This element contains details of a matching group in the matching session.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element name="GroupNum" type="xsd:short">         <xsd:annotation>           <xsd:documentation>The number of this matching group.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="UserRefs" type="mat:UserRefs">         <xsd:annotation>           <xsd:documentation>This element contains user references that will be copied to all document lines in the current matching group.</xsd:do