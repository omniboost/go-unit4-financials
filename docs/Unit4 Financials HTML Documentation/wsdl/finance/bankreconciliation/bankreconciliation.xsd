annotation>       <xsd:documentation>Validates the parameters supplied at run time.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:RequestVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" name="Request" type="brr:ValidateParamsRequest"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="ValidateParamsResponse">     <xsd:complexContent>       <xsd:extension base="com:Response">         <xsd:sequence>           <xsd:element name="CmpCode" type="com:typeGeneralCode">             <xsd:annotation>               <xsd:documentation>The code of the company in which you are performing reconciliation.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element name="MasterCode" type="com:typeGeneralCode">             <xsd:annotation>               <xsd:documentation>The code of the reconciliation master being used for the current reconciliation session.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="ValidateParamsResponseVerb">     <xsd:annotation>       <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:ResponseVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="brr:ValidateParamsResponse"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>        <xsd:complexType name="CommitRequest">     <xsd:complexContent>       <xsd:extension base="com:Request">         <xsd:sequence>           <xsd:element name="ReconciliationParams" type="brr:ReconciliationParams">             <xsd:annotation>               <xsd:documentation>This element contains the parameters which can be specified when reconciling Financials document lines with statement lines.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element name="Groups" type="brr:ReconciliationGroups">             <xsd:annotation>               <xsd:documentation>This element contains details of the reconciliation groups in the reconciliation session.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="CommitRequestVerb">     <xsd:annotation>       <xsd:documentation>Commits the selected reconciliation group. When reconciling Financials document lines with an electronic statement, you cannot commit the reconciliation group until it sums to         zero. When reconciling Financials document lines with a paper statement, there is no need for the reconciliation group to sum to zero.       </xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:RequestVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" name="Request" type="brr:CommitRequest"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="CommitResponse">     <xsd:complexContent>       <xsd:extension base="com:Response">         <xsd:sequence>           <xsd:element name="Results" type="brr:CommitGroupResults">             <xsd:annotation>               <xsd:documentation>This element contains the results of the groups committed by the CommitRequest corresponding to this Response.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="LogID" type="xsd:int">             <xsd:annotation>               <xsd:documentation>A system-generated number identifying the log file in which this reconciliation session has been logged.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="CommitResponseVerb">     <xsd:annotation>       <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:ResponseVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="brr:CommitResponse"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>       <xsd:complexType name="GetWriteOffLimitsRequest">     <xsd:complexContent>       <xsd:extension base="com:Request">         <xsd:sequence>           <xsd:element name="Params" type="brr:ReconciliationWriteOffLimitParams">             <xsd:annotation>               <xsd:documentation>This element contains the parameters which are specified to retrieve credit/debit write off tolerances.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="GetWriteOffLimitsRequestVerb">     <xsd:annotation>       <xsd:documentation>Retrieves the credit/debit write-off tolerances for the specified amount.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:RequestVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" name="Request" type="brr:GetWriteOffLimitsRequest"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="GetWriteOffLimitsResponse">     <xsd:complexContent>       <xsd:extension base="com:Response">         <xsd:sequence>           <xsd:element minOccurs="0" name="CreditWriteOffLimit" type="com:typeMoney">             <xsd:annotation>               <xsd:documentation>The maximum credit amount that may be written off for the specified value.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="DebitWriteOffLimit" type="com:typeMoney">             <xsd:annotation>               <xsd:documentation>The maximum credit amount that may be written off for the specified value.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="GetWriteOffLimitsResponseVerb">     <xsd:annotation>       <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:ResponseVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="brr:GetWriteOffLimitsResponse"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>       <xsd:complexType name="StartBackgroundRequest">     <xsd:complexContent>       <xsd:extension base="com:Request">         <xsd:sequence>           <xsd:element name="ReconciliationParams" type="brr:ReconciliationParams">             <xsd:annotation>               <xsd:documentation>This element contains the parameters which can be specified when reconciling Financials document lines with statement lines.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element name="Selectors" type="brr:ReconciliationSelectors">             <xsd:annotation>               <xsd:documentation>This element contains the selectors to use for selecting Financials document lines and statement lines for the Background Matching process.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="StartBackgroundRequestVerb">     <xsd:annotation>       <xsd:documentation>Starts a Background Reconciliation process.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:RequestVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" name="Request" type="brr:StartBackgroundRequest"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="StartBackgroundResponse">     <xsd:complexContent>       <xsd:extension base="com:Response">         <xsd:sequence>           <xsd:element minOccurs="0" name="ProgressKey" type="prg:ProgressKey">             <xsd:annotation>               <xsd:documentation>Contains a key that identifies the Background Reconciliation process that has been started.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="LogID" type="xsd:int">             <xsd:annotation>               <xsd:documentation>A system-generated number identifying the log file in which this reconciliation session has been logged.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="StartBackgroundResponseVerb">     <xsd:annotation>       <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:ResponseVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="brr:StartBackgroundResponse"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>       <xsd:complexType name="StartAssistantRequest">     <xsd:complexContent>       <xsd:extension base="com:Request">         <xsd:sequence>           <xsd:element name="ReconciliationParams" type="brr:ReconciliationParams">             <xsd:annotation>               <xsd:documentation>This element contains the parameters which can be specified when reconciling Financials document lines with statement lines.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element name="Folders" type="aso:asoKeyFolders">             <xsd:annotation>               <xsd:documentation>This element contains details of the folders in which the Financials document lines and statement lines are listed.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="StartAtGroupNumber" type="xsd:int">             <xsd:annotation>               <xsd:documentation>The group number at which the assistant should start creating reconciliation groups. For example, the user might have manually created groups 1 and 2, and would                 therefore want the Assistant to create new groups starting from 3.               </xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="StartAssistantRequestVerb">     <xsd:annotation>       <xsd:documentation>Starts an Assisted Interactive Reconciliation process.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:RequestVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" name="Request" type="brr:StartAssistantRequest"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="StartAssistantResponse">     <xsd:complexContent>       <xsd:extension base="com:Response">         <xsd:sequence>           <xsd:element minOccurs="0" name="ProgressKey" type="prg:ProgressKey">             <xsd:annotation>               <xsd:documentation>Contains a key that identifies the Assisted Interactive Reconciliation process that has been started.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="LogID" type="xsd:int">             <xsd:annotation>               <xsd:documentation>A system-generated number identifying the log file in which this reconciliation session has been logged.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="StartAssistantResponseVerb">     <xsd:annotation>       <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:ResponseVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="brr:StartAssistantResponse"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>       <xsd:complexType name="GetReconciliationGroupsRequest">     <xsd:complexContent>       <xsd:extension base="com:Request">         <xsd:sequence>           <xsd:element minOccurs="0" name="ProgressKey" type="prg:ProgressKey">             <xsd:annotation>               <xsd:documentation>Contains a key that identifies the Assistant process for which you want to retrieve reconciliation group information.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="GetReconciliationGroupsRequestVerb">     <xsd:annotation>       <xsd:documentation>Retrieves information about the reconciliation groups created by the specified Assistant process.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:RequestVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" name="Request" type="brr:GetReconciliationGroupsRequest"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="GetReconciliationGroupsResponse">     <xsd:complexContent>       <xsd:extension base="com:Response">         <xsd:sequence>           <xsd:element name="Groups" type="brr:ReconciliationGroups">             <xsd:annotation>               <xsd:documentation>This element contains details of the groups in the reconciliation session.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="GetReconciliationGroupsResponseVerb">     <xsd:annotation>       <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:ResponseVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="brr:GetReconciliationGroupsResponse"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>       <xsd:complexType name="ProgressResponse">     <xsd:complexContent>       <xsd:extension base="prg:progressGetResponse">         <xsd:sequence>           <xsd:element minOccurs="0" name="Statistics" type="aso:ProgressStatistics">             <xsd:annotation>               <xsd:documentation>This element contains statistics of the lines that the Assistant has processed so far.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="ProgressResponseVerb">     <xsd:annotation>       <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:ResponseVerb"