ent>           <xsd:element name="Groups" type="brr:UndoGroupResults">             <xsd:annotation>               <xsd:documentation>This element contains the results of the UndoRequest corresponding to this Response.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="UndoResponseVerb">     <xsd:annotation>       <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:ResponseVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="brr:UndoResponse"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>       <xsd:complexType name="BankReconciliationRequest">  <xsd:annotation>        <xsd:appinfo build:funcSecValue="368" build:readOnly="false"/>        <xsd:documentation>Bank reconciliation service.</xsd:documentation>  </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:ServiceRequest">         <xsd:sequence>           <xsd:element minOccurs="0" name="ValidateParams" type="brr:ValidateParamsRequestVerb">             <xsd:annotation>      <xsd:appinfo build:funcSecValue="403" build:readOnly="true"/>               <xsd:documentation>Validates the parameters supplied at run time.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="Commit" type="brr:CommitRequestVerb">             <xsd:annotation>      <xsd:appinfo build:funcSecValue="349" build:readOnly="false"/>               <xsd:documentation>Commits the selected reconciliation group. When reconciling Financials document lines with an electronic statement, you cannot commit the reconciliation group until it                 sums to zero. When reconciling Financials document lines with a paper statement, there is no need for the reconciliation group to sum to zero.               </xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="GetWriteOffLimits" type="brr:GetWriteOffLimitsRequestVerb">             <xsd:annotation>      <xsd:appinfo build:funcSecValue="350" build:readOnly="true"/>               <xsd:documentation>Retrieves the credit/debit write-off tolerances for the specified amount.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="StartBackground" type="brr:StartBackgroundRequestVerb">             <xsd:annotation>      <xsd:appinfo build:funcSecValue="351" build:readOnly="false"/>               <xsd:documentation>Starts a Background Reconciliation process.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="StartAssistant" type="brr:StartAssistantRequestVerb">             <xsd:annotation>      <xsd:appinfo build:funcSecValue="352" build:readOnly="false"/>               <xsd:documentation>Starts an Assisted Interactive Reconciliation process.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="GetReconciliationGroups" type="brr:GetReconciliationGroupsRequestVerb">             <xsd:annotation>      <xsd:appinfo build:funcSecValue="353" build:readOnly="true"/>               <xsd:documentation>Retrieves information about the reconciliation groups created by the specified Assistant process.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="GetProgress" type="prg:progressGetRequestVerb">             <xsd:annotation>      <xsd:appinfo build:funcSecValue="354" build:readOnly="true"/>               <xsd:documentation>Retrieves information about the number of lines that have been processed by the current Assistant process.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="Abort" type="prg:progressAbortRequestVerb">             <xsd:annotation>      <xsd:appinfo build:funcSecValue="355" build:readOnly="false"/>               <xsd:documentation>Stops the current Assistant process. If the process was Assisted Interactive Reconciliation, any reconciliation groups already suggested are retained. If the process                 was Background Reconciliation, any reconciliation groups already produced are discarded and no groups are committed.               </xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="Undo" type="brr:UndoRequestVerb">             <xsd:annotation>      <xsd:appinfo build:funcSecValue="356" build:readOnly="false"/>               <xsd:documentation>Undoes the specified reconciliation group(s) using the parameters you provide.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="BankReconciliationResponse">     <xsd:complexContent>       <xsd:extension base="com:ServiceResponse">         <xsd:sequence>           <xsd:element minOccurs="0" name="ValidateParams" type="brr:ValidateParamsResponseVerb">             <xsd:annotation>               <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="Commit" type="brr:CommitResponseVerb">             <xsd:annotation>               <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="GetWriteOffLimits" type="brr:GetWriteOffLimitsResponseVerb">             <xsd:annotation>        