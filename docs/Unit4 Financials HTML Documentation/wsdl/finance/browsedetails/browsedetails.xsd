ment>           <xsd:element name="TxnKey" type="txn:txnKey">             <xsd:annotation>               <xsd:documentation>Identifies the transaction for which related transactions should be retrieved.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="RowLimit" type="com:typeULong">             <xsd:annotation>               <xsd:documentation>The maximum number of lines that can be retrieved.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element name="Columns" type="sel:HeaderColumnList">             <xsd:annotation>               <xsd:documentation>This element holds a list of columns to retrieve. Each cell is of the type com:typeCtVocabEnum.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="GetRelatedRequestVerb">     <xsd:annotation>       <xsd:documentation>Retrieves the document details from the database using the transaction key.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:RequestVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" name="Request" type="brs:GetRelatedRequest"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="GetRelatedResponse">     <xsd:complexContent>       <xsd:extension base="com:Response">         <xsd:sequence>           <xsd:element name="TxnKey" type="txn:txnKey">             <xsd:annotation>               <xsd:documentation>Identifies the transaction for which related transactions should be retrieved.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="SelectionWithFlexiFields" type="sel:SelectionWithFlexiFields">             <xsd:annotation>               <xsd:documentation>This element contains the document details retrieved by Browse Details.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="GetRelatedResponseVerb">     <xsd:annotation>       <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:ResponseVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="brs:GetRelatedResponse"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>      <xsd:complexType name="UpdateRequest">     <xsd:complexContent>       <xsd:extension base="com:Request">         <xsd:sequence>           <xsd:element name="TxnKey" type="brs:UpdateHeader">             <xsd:annotation>               <xsd:documentation>Identifies the document line to update.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element name="Updates" type="brs:UpdateUnionList">             <xsd:annotation>               <xsd:documentation>Contains the document line details you want to update.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="UpdateRequestVerb">     <xsd:annotation>       <xsd:documentation>Updates the document line details in the database, using the information you provide.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:RequestVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" name="Request" type="brs:UpdateRequest"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="UpdateResponse">     <xsd:complexContent>       <xsd:extension base="com:Response">         <xsd:sequence>           <xsd:element name="TxnKey" type="brs:UpdateHeader">             <xsd:annotation>               <xsd:documentation>Identifies the transaction which has been updated.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="UpdateResponseVerb">     <xsd:annotation>       <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:ResponseVerb">         <xsd:sequence>   <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="brs:UpdateResponse"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>       <xsd:complexType name="AllowEditRequest">     <xsd:complexContent>       <xsd:extension base="com:Request">         <xsd:sequence>           <xsd:element name="Key" type="txn:txnLineKey">             <xsd:annotation>               <xsd:documentation>Identifies the document line to check for edit permission.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="AllowEditRequestVerb">     <xsd:annotation>       <xsd:documentation>Checks the document line details in the database, using the information you provide.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:RequestVerb">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" name="Request" type="brs:AllowEditRequest"/>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="AllowEditResponse">     <xsd:complexContent>       <xsd:extension base="com:Response"> 