 <xsd:documentation>A generic key consisting of the company code and the archive header master code.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ExternalArchive" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>If TRUE specifies that this is an external archive.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="FromPeriod" type="com:typePeriod">     <xsd:annotation>      <xsd:documentation>The first year/period for which documents are being archived.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="ToPeriod" type="com:typePeriod">     <xsd:annotation>      <xsd:documentation>The last year/period for which documents are being archived.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="LastDocumentCode" type="com:typeGeneralCode">     <xsd:annotation>      <xsd:documentation>The code of the most recent document to be processed.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="LastDocumentNumber" type="com:typeDocNum">     <xsd:annotation>      <xsd:documentation>The number of the most recent document to be processed.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="LastDocumentCount" type="xsd:integer">     <xsd:annotation>      <xsd:documentation>The number of documents archived so far.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="LastJournalCode" type="com:typeGeneralCode">     <xsd:annotation>      <xsd:documentation>The code of the most recent journal written.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="LastJournalNumber" type="com:typeDocNum">     <xsd:annotation>      <xsd:documentation>The number of the most recent journal written.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="LastJournalCount" type="xsd:integer">     <xsd:annotation>      <xsd:documentation>The number of journals written so far.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="RunTime" type="arc:ArchiveRunTime">     <xsd:annotation>      <xsd:documentation>This element contains the accumulated run time since the archive process began.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="CurrentState" type="com:typeCtArchiveState">     <xsd:annotation>      <xsd:documentation>The current status of the archive process, for example &apos;Selecting documents&apos;.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="Paused" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>If TRUE indicates that the archive process has paused.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="LogID" type="xsd:int">     <xsd:annotation>      <xsd:documentation>The identifier for the log containing details of the Archive process.</xsd:documentation>     </xsd:annotation>    </xsd:element>   </xsd:sequence>  </xsd:complexType>   <xsd:element name="ArchiveDocumentsInformation" type="arc:ArchiveDocumentsInformation">   <xsd:annotation>    <xsd:documentation>This element holds information about archiving documents, and the state of the data during an Archive process.</xsd:documentation>   </xsd:annotation>  </xsd:element>  <xsd:element name="ArchiveMonitor" type="arc:ArchiveMonitor">   <xsd:annotation>    <xsd:documentation>This element holds information about the progress of the archive process.</xsd:documentation>   </xsd:annotation>  </xsd:element>                 <xsd:complexType name="StartArchivingDocumentsRequest">   <xsd:complexContent>    <xsd:extension base="com:Request">     <xsd:sequence>      <xsd:element name="ArchiveDocumentsParams" type="arc:ArchiveDocumentsParams">       <xsd:annotation>        <xsd:documentation>Contains the parameters for starting the Archive Documents process.</xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="StartArchivingDocumentsRequestVerb">   <xsd:annotation>    <xsd:documentation>Starts the Archive Documents process.</xsd:documentation>   </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:RequestVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" name="Request" type="arc:StartArchivingDocumentsRequest"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="StartArchivingDocumentsResponse">   <xsd:complexContent>    <xsd:extension base="com:Response">     <xsd:sequence>      <xsd:element name="CmpCode" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The code of the company in which you are attempting to archive documents.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="ArchiveHeaderMasterCode" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The code of the archive header master you are using to archive documents.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="BalanceWarning" type="arc:BalanceWarning">       <xsd:annotation>        <xsd:documentation>A warning that not all documents for the year have been archived. Archive Documents should be run before archiving balances.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="ProgressKey" type="prg:ProgressKey">       <xsd:annotation>        <xsd:documentation>The key, or special reference, given to you for requesting information about the progress of Archive Documents.</xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>        </xsd:complexType>   <xsd:complexType name="StartArchivingDocumentsRespons