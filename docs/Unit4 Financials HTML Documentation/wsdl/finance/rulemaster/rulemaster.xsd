nceSource">         <xsd:annotation>           <xsd:documentation>External reference 1.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="ExternalReference2" type="com:typeCtRuleReferenceSource">         <xsd:annotation>           <xsd:documentation>External reference 2.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="ExternalReference3" type="com:typeCtRuleReferenceSource">         <xsd:annotation>           <xsd:documentation>External reference 3.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="ExternalReference4" type="com:typeCtRuleReferenceSource">         <xsd:annotation>           <xsd:documentation>External reference 4.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="ExternalReference5" type="com:typeCtRuleReferenceSource">         <xsd:annotation>           <xsd:documentation>External reference 5.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="ExternalReference6" type="com:typeCtRuleReferenceSource">         <xsd:annotation>           <xsd:documentation>External reference 6.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="Comment" type="com:typeCtRuleReferenceSource">         <xsd:annotation>           <xsd:documentation>Comment.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType abstract="true" name="Master">     <xsd:sequence>       <xsd:element name="TimeStamp" type="xsd:short">         <xsd:annotation>           <xsd:documentation>The TimeStamp value for this rule master in the database.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="CmpCode" type="com:typeGeneralCodeB">         <xsd:annotation>           <xsd:documentation>The code of the company in which this rule master exists.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="Code" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The code for this rule master.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="Name" type="com:typeName">         <xsd:annotation>           <xsd:documentation>The full name of the rule master.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="ShortName" type="com:typeShortNameB">         <xsd:annotation>           <xsd:documentation>The abbreviated name of the rule master.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="SelectorCode" type="com:typeGeneralCodeB">         <xsd:annotation>           <xsd:documentation>The code of the selector master used by this rule to select the values.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="DistributionCode" type="com:typeGeneralCodeB">         <xsd:annotation>           <xsd:documentation>The code of the distribution master containing the list of accounts to be updated.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="PresenterCode" type="com:typeGeneralCodeB">         <xsd:annotation>           <xsd:documentation>The code of the presenter master used with this rule, if required.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="Destination" type="com:typeCtSlnSource">         <xsd:annotation>           <xsd:documentation>The location which is updated, or to which the values are allocated. This may be Balances, Books, Intray, or Try Books, then Intray.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="HomeValue" type="com:typeNum">         <xsd:annotation>           <xsd:documentation>A fixed value in home currency to be allocated instead of a 