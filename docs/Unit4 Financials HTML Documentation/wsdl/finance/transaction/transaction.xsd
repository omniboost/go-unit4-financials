lexType>  <xsd:complexType name="Currencies">   <xsd:annotation>    <xsd:documentation>This element holds the element currency information for the document line.</xsd:documentation>   </xsd:annotation>   <xsd:sequence>    <xsd:element maxOccurs="13" minOccurs="0" name="Currency" type="txn:Currency">     <xsd:annotation>      <xsd:documentation>This element holds the currency information for one element currency.</xsd:documentation>     </xsd:annotation>    </xsd:element>   </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="WhichCurrs">   <xsd:annotation>    <xsd:documentation>This element holds details of where a currency is used. For example, the same currency might be used as the home currency, document currency and element 3&apos;s currency.</xsd:documentation>   </xsd:annotation>   <xsd:sequence>    <xsd:element maxOccurs="13" minOccurs="0" name="Curr" type="com:typeCtWhichCur">     <xsd:annotation>      <xsd:documentation>Identifies where the specified currency is used.</xsd:documentation>     </xsd:annotation>    </xsd:element>   </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="CurrencyInfo">   <xsd:sequence>    <xsd:element name="Code" type="com:typeGeneralCode"/>    <xsd:element name="Which" type="txn:WhichCurrs"/>    <xsd:element name="Value" type="com:typeMoney"/>    <xsd:element name="ValidRate" type="xsd:boolean"/>    <xsd:element name="LinkType" type="com:typeCtCurLinkType"/>    <xsd:element minOccurs="0" name="ParentCode" type="com:typeGeneralCode"/>    <xsd:element name="DecimalPlaces" type="com:typeWord"/>   </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="HeaderWorkflow">   <xsd:annotation>    <xsd:documentation>This element holds details of the workflow used by this document.</xsd:documentation>   </xsd:annotation>   <xsd:sequence>    <xsd:element minOccurs="0" name="WorkflowType" type="com:typeCtWFlowType">     <xsd:annotation>      <xsd:documentation>Whether the workflow is set up for document-wide or line-wide auth