e>    <xsd:element maxOccurs="5" minOccurs="5" name="Discount" type="txn:Discount">     <xsd:annotation>      <xsd:documentation>This element holds details of a discount rate available to this document.</xsd:documentation>     </xsd:annotation>    </xsd:element>   </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="RecurringDiscounts">   <xsd:annotation>    <xsd:documentation>This element holds details of discount rates that are available if this document is a recurring document.</xsd:documentation>   </xsd:annotation>   <xsd:sequence>    <xsd:element maxOccurs="5" minOccurs="0" name="Discount" type="txn:RecurringDiscount">     <xsd:annotation>      <xsd:documentation>This element holds details of a discount rate that is available if this document is a recurring document.</xsd:documentation>     </xsd:annotation>    </xsd:element>   </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="ElmQuantities">   <xsd:annotation>    <xsd:documentation>This element holds the quantity information for the element at level 1.</xsd:documentation>   </xsd:annotation>   <xsd:sequence>    <xsd:element minOccurs="0" name="Quantity1" type="com:typeNum">     <xsd:annotation>      <xsd:documentation>The amount for quantity 1.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="Quantity2" type="com:typeNum">     <xsd:annotation>      <xsd:documentation>The amount for quantity 2.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="Quantity3" type="com:typeNum">     <xsd:annotation>      <xsd:documentation>The amount for quantity 3.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="Quantity4" type="com:typeNum">     <xsd:annotation>      <xsd:documentation>The amount for quantity 4.</xsd:documentation>     </xsd:annotation>    </xsd:element>   </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="Comments">   <xsd:annotation>    <xsd:documentation>This element holds comments on the document.</xsd:documentation>   </xsd:annotation>   <xsd:sequence>    <xsd:element maxOccurs="unbounded" minOccurs="0" name="Comment" type="com:typeTextB">     <xsd:annotation>      <xsd:documentation>A comment line on the document.</xsd:documentation>     </xsd:annotation>    </xsd:element>   </xsd:sequence>  </xsd:complexType>   <xsd:complexType name="Asset">   <xsd:annotation>    <xsd:documentation>This element holds details of the asset that will be created from a document line when the document is posted. If the element and document master flags cause an asset to be generated on a line, the information in this structure can be used to configure it</xsd:documentation>   </xsd:annotation>   <xsd:sequence>    <xsd:element minOccurs="0" name="CategoryCode" type="com:typeGeneralCodeB">     <xsd:annotation>      <xsd:documentation>The code of the asset category which provides the default values for the asset.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="AssetCode" type="com:typeAstCodeB">     <xsd:annotation>      <xsd:documentation>The code of the asset.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="ParentAssetCode" type="com:typeAstCodeB">     <xsd:annotation>      <xsd:documentation>The code of the parent asset (if any).</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="CreateOnRegister" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Indicates whether the asset is posted directly to the Asset Register, resulting in the asset being capitalised immediately.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="InheritRemainingLife" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>Indicates whether the asset inherits the parent asset&apos;s remaining asset life. When TRUE, the child asset will share the s