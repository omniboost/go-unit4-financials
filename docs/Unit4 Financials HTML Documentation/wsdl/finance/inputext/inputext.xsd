ent>           <xsd:element minOccurs="0" name="PositionCode" type="com:typeGeneralCode">             <xsd:annotation>               <xsd:documentation>The position code within the position hierarchy.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="IntrayKeys">     <xsd:annotation>       <xsd:documentation>Contains key information identifying which document(s) on the Intray will be deleted or unauthorised.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element maxOccurs="unbounded" name="Key" type="txn:txnKey">         <xsd:annotation>           <xsd:documentation>Contains a key uniquely identifying a document within the Financials database.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="IntrayResult">     <xsd:annotation>       <xsd:documentation>Contains results information for the documents deleted from the Intray.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:Response">         <xsd:sequence>           <xsd:element name="Key" type="txn:txnKey">             <xsd:annotation>               <xsd:documentation>Contains a key uniquely identifying a document within the Financials database.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="IntrayPostResult">     <xsd:annotation>       <xsd:documentation>Contains results information for a document posted from the Intray to the Books.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:Response">         <xsd:sequence>           <xsd:element name="Key" type="inx:IntrayPostKey">             <xsd:annotation>               <xsd:documentation>Contains key information for a document on the Intray that has been posted to the Books.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="IntrayDeleteInfo">     <xsd:annotation>       <xsd:documentation>Contains the reason why the specified document on the Intray is being deleted.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element name="Reason" type="com:typeText">         <xsd:annotation>           <xsd:documentation>The reason why the specified document on the Intray is being deleted.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="IncludeInterCompanyDocs" type="com:typeBool">         <xsd:annotation>           <xsd:documentation>Indicates that the user is willing to delete Intercompany documents from the Intray.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="IntrayItem">     <xsd:annotation>       <xsd:documentation>Contains a document on the Intray that has been retrieved.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element name="CmpCode" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The code of the company in which the document is held.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="Code" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The document master code.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="Number" type="com:typeDocNumAlpha">         <xsd:annotation>           <xsd:documentation>The document number.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="Date" type="com:typeDate">         <xsd:annotation>           <xsd:documentation>The document date.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="Period" type="com:typePeriod">         <xsd:annotation>           <xsd:documentation>The year and period to which the document is to be posted.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="OriginalUser" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The code of the user who created the document.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="LastModBy" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The code of the user who last modified the document.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="AuthInitials" type="com:typeAuthInit">         <xsd:annotation>           <xsd:documentation>The authorising initials.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="LastModDate" type="com:typeDate">         <xsd:annotation>           <xsd:documentation>The date when the document was last modified.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="FirstDetailValue" type="com:typeMoney">         <xsd:annotation>           <xsd:documentation>The value of the first line on the document.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="FirstAccountCode" type="com:typeAcCode">         <xsd:annotation>           <xsd:documentation>The account code of the first line on the document.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="IsInterCompany" type="xsd:boolean">         <xsd:annotation>           <xsd:documentation>Indicates whether the document is an InterCompany document.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="IsPODNotDeletable" type="xsd:boolean">         <xsd:annotation>           <xsd:documentation>This element will be set to TRUE if the document has been matched to a purchasing ordering document, and is therefore not deletable.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="AuthorisationStatus" type="com:typeCtWFlowHdrStatusNoAny">         <xsd:annotation>           <xsd:documentation>The current workflow authorisation status of the document.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="CanApplyCurrentRate" type="xsd:boolean">         <xsd:annotation>           <xsd:documentation>Specifies whether current exchange rates can be applied to this document when posting.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="Template" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The code of the input template master used when inputting the document.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="CompletionUser" type="com:typeGeneralCodeB">         <xsd:annotation>           <xsd:documentation>The user code for the completing user.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="CompletionStatus" type="com:typeCtWFlowCompStatus">         <xsd:annotation>           <xsd:documentation>The workflow completion status.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="RequiresBooksWorkflow" type="xsd:boolean">         <xsd:annotation>           <xsd:documentation>Indicates that this document requires workflow authorisation before posting to the Books.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="BooksWorkflowCode" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The workflow process definition code for use when posting to the Books.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="BooksWorkflowProtected" type="xsd:boolean">         <xsd:annotation>           <xsd:documentation>Indicates whether the workflow process definition code is protected.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="PositionHierarchyCode" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The position code within the position hierarchy.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="IntrayItems">     <xsd:annotation>       <xsd:documentation>Contains the documents on the Intray that have been retrieved.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:Response">         <xsd:sequence>           <xsd:element maxOccurs="unbounded" minOccurs="0" name="Item" type="inx:IntrayItem">             <xsd:annotation>               <xsd:documentation>Contains a document on the Intray that has been retrieved.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="IntrayFilter">     <xsd:annotation>       <xsd:documentation>Contains criteria for selecting documents on the Intray.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element name="CmpCode" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The code of the company in which you are selecting documents on the Intray.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="Limit" type="com:typeULongB">         <xsd:annotation>           <xsd:documentation>The maximum number of documents to return.</xsd:docu