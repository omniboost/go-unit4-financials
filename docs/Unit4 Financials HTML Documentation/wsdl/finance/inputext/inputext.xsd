<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:com="http://www.coda.com/efinance/schemas/common" xmlns:inx="http://www.coda.com/efinance/schemas/inputext" xmlns:elm="http://www.coda.com/efinance/schemas/elementmaster" xmlns:txn="http://www.coda.com/efinance/schemas/transaction" xmlns:inp="http://www.coda.com/efinance/schemas/input" xmlns:itm="http://www.coda.com/efinance/schemas/inputtemplate" xmlns:doc="http://www.coda.com/efinance/schemas/documentmaster" xmlns:build="http://www.coda.com/common/schemas/funcsec" xmlns:mat="http://www.coda.com/efinance/schemas/matching" targetNamespace="http://www.coda.com/efinance/schemas/inputext" elementFormDefault="qualified"><xsd:import namespace="http://www.coda.com/efinance/schemas/input" schemaLocation="../../../wsdl/finance/input/input.xsd"/><xsd:import namespace="http://www.coda.com/efinance/schemas/transaction" schemaLocation="../../../wsdl/finance/transaction/transaction.xsd"/><xsd:import namespace="http://www.coda.com/efinance/schemas/common" schemaLocation="../../../wsdl/finance/common/common.xsd"/><xsd:import namespace="http://www.coda.com/efinance/schemas/elementmaster" schemaLocation="../../../wsdl/finance/elementmaster/elementmaster.xsd"/><xsd:import namespace="http://www.coda.com/efinance/schemas/inputtemplate" schemaLocation="../../../wsdl/finance/inputtemplate/inputtemplate.xsd"/><xsd:import namespace="http://www.coda.com/efinance/schemas/documentmaster" schemaLocation="../../../wsdl/finance/documentmaster/documentmaster.xsd"/><xsd:import namespace="http://www.coda.com/efinance/schemas/matching" schemaLocation="../../../wsdl/finance/matching/matching.xsd"/>                    <xsd:complexType name="ElmReviewItem">     <xsd:annotation>       <xsd:documentation>This element contains the total value (in document, home and dual currencies) for a balancing element in the document.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element name="BalElmCode" type="com:typeAcCode">         <xsd:annotation>           <xsd:documentation>The code of the balancing element. This is a string containing either a single Level1 element, or a Level1.Level2 element combination, that identifies a company set up to use balancing elements.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="DocValue" type="com:typeMoney">         <xsd:annotation>           <xsd:documentation>The total value for this balancing element in document currency.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="HomeValue" type="com:typeMoney">         <xsd:annotation>           <xsd:documentation>The total value for this balancing element in home currency.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="DualValue" type="com:typeMoney">         <xsd:annotation>           <xsd:documentation>The total value for this balancing element in dual currency.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="ElmReview">     <xsd:annotation>       <xsd:documentation>This element contains the total value (in document, home and dual currencies) for the balancing elements in the document.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element maxOccurs="unbounded" minOccurs="0" name="Item" type="inx:ElmReviewItem">         <xsd:annotation>           <xsd:documentation>This element contains the total value (in document, home and dual currencies) for a balancing element in the document.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="BalancingCurrReview">     <xsd:annotation>       <xsd:documentation>This element contains the total document value in document, home and dual currencies.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element name="DocTotal" type="com:CurrValue">         <xsd:annotation>           <xsd:documentation>The total document value in document currency.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="HomeTotal" type="com:CurrValue">         <xsd:annotation>           <xsd:documentation>The total document value in home currency.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="DualTotal" type="com:CurrValue">         <xsd:annotation>           <xsd:documentation>The total document value in dual currency.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="BalancingReview">     <xsd:annotation>       <xsd:documentation>Contains the sum of values on the specified document for each balancing element and the whole document (in document, home and dual currencies).</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element name="CurrReview" type="inx:BalancingCurrReview">         <xsd:annotation>           <xsd:documentation>This element contains the total document value in document, home and dual currencies.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="ElmReview" type="inx:ElmReview">         <xsd:annotation>           <xsd:documentation>This element contains the total value (in document, home and dual currencies) for the balancing elements in the document.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="CurrReviewCurrRateValue">     <xsd:annotation>       <xsd:documentation>This element contains details about a currency that applies to all lines in the document.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:CurrRateValue">         <xsd:sequence>           <xsd:element minOccurs="0" name="Which" type="txn:WhichCurrs">             <xsd:annotation>               <xsd:documentation>This element holds details of where the currency is used. For example, it might be used as the home currency, document currency and element 3&apos;s currency.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="CurrReviewCurrRateValueList">     <xsd:annotation>       <xsd:documentation>This element contains document-wide exchange rates and document totals. If the document is not configured to use document-wide exchange rates, the Rate element within this list is omitted.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element maxOccurs="unbounded" minOccurs="0" name="Currency" type="inx:CurrReviewCurrRateValue">         <xsd:annotation>           <xsd:documentation>This element contains details about a currency that applies to all lines in the document.           A given currency will occur at most once in the following list. The first element in the following list will always be       for the document currency. The first element will be followed by an element for the home currency if this differs       from the document currency. This will be followed by the dual currency if it exists and is not equal to the document       currency. This will be followed by other currencies on the transaction.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="CurrReviewLine">     <xsd:annotation>       <xsd:documentation>This element contains a list of the currency details that apply to a line in the document.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element name="AccountCode" type="com:typeAcCode">         <xsd:annotation>           <xsd:documentation>The account code on the document line.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="Currencies" type="inx:CurrReviewCurrRateValueList">         <xsd:annotation>           <xsd:documentation>This element contains a list of the currencies used on this document line.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="CurrReviewLineList">     <xsd:annotation>       <xsd:documentation>This element contains a list of the currency details that apply to lines in the document.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element maxOccurs="unbounded" minOccurs="0" name="Line" type="inx:CurrReviewLine">         <xsd:annotation>           <xsd:documentation>This element contains a list of the currency details that apply to a line in the document.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="CurrReview">     <xsd:annotation>       <xsd:documentation>This element contains the currency rates and currency values of all lines in the specified document.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element name="DocumentWideData" type="inx:CurrReviewCurrRateValueList">         <xsd:annotation>           <xsd:documentation>This element contains document-wide exchange rates and document totals. If the document is not configured to use document-wide exchange rates, the Rate element within this list is omitted.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="LineData" type="inx:CurrReviewLineList">         <xsd:annotation>           <xsd:documentation>This element contains a list of the currency details that apply to lines in the document.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="CustomerSupplierElmSummaryLine">     <xsd:annotation>       <xsd:documentation>This element contains summary information about the customer/supplier element on the specified document line.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element name="LineNumber" type="xsd:int">         <xsd:annotation>           <xsd:documentation>The document line number.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="ElmCode" type="com:typeElmCode">         <xsd:annotation>           <xsd:documentation>The code of the customer/supplier element.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="ElmLevel" type="com:typeElmLevel">         <xsd:annotation>           <xsd:documentation>The element level of the customer/supplier element.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="ShortName" type="com:typeShortName">         <xsd:annotation>           <xsd:documentation>The customer/supplier element&apos;s short name.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="DocValue" type="com:typeMoney">         <xsd:annotation>           <xsd:documentation>The value (in document currency) of this document line.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="LineSense" type="com:typeCtDocLineDrCrB">         <xsd:annotation>           <xsd:documentation>The sense of this line (debit, credit or none).</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="CustomerSupplierElmSummary">     <xsd:annotation>       <xsd:documentation>This element contains summary information about the customer/supplier elements in the specified document.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element maxOccurs="unbounded" minOccurs="0" name="Line" type="inx:CustomerSupplierElmSummaryLine">         <xsd:annotation>           <xsd:documentation>This element contains summary information about the customer/supplier element on the specified document line.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="SelectedDocList">     <xsd:annotation>       <xsd:documentation>When cancelling multiple documents, this contains the list of documents to be cancelled. The selected documents must fall within the range specified by DocNumberFrom and DocNumberTo. The document numbers must be listed in ascending order and must not be duplicated.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element maxOccurs="unbounded" minOccurs="0" name="DocNumber" type="com:typeDocNum">         <xsd:annotation>           <xsd:documentation>The document number of a document selected to be cancelled.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="CancelDoc">     <xsd:annotation>       <xsd:documentation>Contains parameters which can be specified when cancelling a single document or multiple documents.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element name="CmpCode" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The code of the company in which you want to cancel documents.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="CancellingDocCode" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The code of the document master set up to produce cancelling documents.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="DocCode" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The code of the document master used to input the document(s) you wish to cancel.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="DocNumberFrom" type="com:typeDocNum">         <xsd:annotation>           <xsd:documentation>The document number of the single document you want to cancel, or the first document number in a range if you want to cancel multiple documents.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="DocNumberTo" type="com:typeDocNum">         <xsd:annotation>           <xsd:documentation>The last document number in the range if you want to cancel multiple documents. This can be blank if you are cancelling a single document.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="SelectedDocs" type="inx:SelectedDocList">         <xsd:annotation>           <xsd:documentation>When cancelling multiple documents, this contains the list of documents to be cancelled. The selected documents must fall within the range specified by DocNumberFrom and DocNumberTo. The document numbers must be listed in ascending order and must not be duplicated.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="DocDate" type="com:typeDate">         <xsd:annotation>           <xsd:documentation>The date to be used as the document date for the cancelling documents. If this is left blank, the original document date is used.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="Period" type="com:typePeriod">         <xsd:annotation>           <xsd:documentation>The year and period to which the cancelling documents are posted. If this is left blank, the original document year/period is used.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="IncludeInterCompanyDocs" type="com:typeBool">         <xsd:annotation>           <xsd:documentation>When true, any documents using InterCompany (that is, containing accounts in more than one company) will be cancelled if selected by the cancellation criteria. When false, no InterCompany documents will be cancelled. If this element is omitted, the cancellation request will abort with a reason message in CancelDocResults/InterCompanyWarning if any InterCompany documents are found.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="LogTitle" type="com:typeLogTitle">         <xsd:annotation>           <xsd:documentation>An optional log title. All attempts to cancel the specified documents are stored under this log title.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="WorkflowCode" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The workflow process definition code.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="PositionCode" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The position code within the position hierarchy.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="CancelDocInterCompanyWarning">     <xsd:annotation>       <xsd:documentation>Contains a reason message if any InterCompany documents were found but IncludeInterCompanyDocs was not supplied in the request.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element name="Reason" type="com:Reason">         <xsd:annotation>           <xsd:documentation>A message explaining why the cancellation request has been aborted.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="CancelDocResults">     <xsd:annotation>       <xsd:documentation>Contains the results of a request to cancel one or more documents.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element name="CmpCode" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The code of the company in which documents were cancelled.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="DocCode" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The document master code of the documents that were cancelled.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="NumberOfDocsSelected" type="xsd:int">         <xsd:annotation>           <xsd:documentation>The number of document numbers sent in the request if SelectedDocs is present, or 1 if the request was to cancel a single document.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="NumberOfDocsFound" type="xsd:int">         <xsd:annotation>           <xsd:documentation>The number of documents found.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="NumberOfUncancelledDocsFound" type="xsd:int">         <xsd:annotation>           <xsd:documentation>The number of documents found that have not already been cancelled.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="NumberOfDocsCancelled" type="xsd:int">         <xsd:annotation>           <xsd:documentation>The number of documents actually cancelled.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="InterCompanyWarning" type="inx:CancelDocInterCompanyWarning">         <xsd:annotation>           <xsd:documentation>Contains a reason message if any InterCompany documents were found but IncludeInterCompanyDocs was not supplied in the request.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="WorkflowStarted" type="xsd:boolean">         <xsd:annotation>           <xsd:documentation>Indicates that the workflow process has started.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="SpecialDocPostData">     <xsd:sequence>       <xsd:element name="RateRule" type="com:typeCtCurrencyRule">         <xsd:annotation>           <xsd:documentation>Specifies whether the generated document is posted using the currency exchange rates as specified on the original document (equal), or the currency exchange rates in effect on the reversing document date (calculated).</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="ReversingPostData">     <xsd:annotation>       <xsd:documentation>Contains details that can be specified when posting reversing documents, or when checking that reversing documents are postable.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="inx:SpecialDocPostData">         <xsd:sequence>           <xsd:element name="DocDate" type="com:typeDate">             <xsd:annotation>               <xsd:documentation>The document date for the reversing entry. This defaults to the date of the original document.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element name="Period" type="com:typePeriod">             <xsd:annotation>               <xsd:documentation>The year and period to which the reversing entry will be posted. This defaults to the period following that of the original document. If the period on the original document is the last posting period of the year, the reversing entry will be posted to the first posting period in the new year.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element name="Match" type="com:typeBool">             <xsd:annotation>               <xsd:documentation>When TRUE, lines on the reversing document are automatically matched against lines on the original document.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="RecurringPostData">     <xsd:annotation>       <xsd:documentation>Contains details that can be specified when posting recurring documents, or when checking that recurring documents are postable.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="inx:SpecialDocPostData">         <xsd:sequence>           <xsd:element name="NumberOfDocs" type="com:typeULong">             <xsd:annotation>               <xsd:documentation>The total number of documents required, including the original document.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element name="Phasing" type="com:typePhasing">             <xsd:annotation>               <xsd:documentation>The time lapse between the document date of each document. This determines when each recurring document will be posted. Here you specify a soft date consisting of four characters with the initial character representing days, months or periods. For example, D007 indicates 7 days between postings, M002 indicates 2 months, P003 indicates 3 periods.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element name="DateRule" type="com:typeDateRule">             <xsd:annotation>               <xsd:documentation>The date rule to use for determining the document date of the recurring documents. The rule you can specify depends on what you have set for Phasing.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element name="AmountRule" type="com:typeCtAmountRule">             <xsd:annotation>               <xsd:documentation>Determines the value of the recurring documents. The value of the original document can be divided equally between the recurring documents (divided), or each recurring document can have the same value as the original document (equal). If you set this to null, the default is applied which is equal.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="DocWideDueTerms" type="com:typeTerm">             <xsd:annotation>               <xsd:documentation>A valid soft date determining the due date. This defaults to the value from the document master (if one exists) or the value from the element master. It is read-only if the document master has DueDate/DocumentWide set to TRUE, or if DueDate/Modifiable is set to not modifiable.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="DocWideValueTerms" type="com:typeTerm">             <xsd:annotation>               <xsd:documentation>A valid soft date determining the value date. This defaults to the value from the document master (if one exists) or is blank. It is read-only if the document master has ValueDate/DocumentWide set to TRUE, or if ValueDate/Modifiable is set to not modifiable.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="DocumentWideData">     <xsd:annotation>       <xsd:documentation>This element contains details that apply to all lines in the document.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element minOccurs="0" name="DueDate" type="com:typeDateB">         <xsd:annotation>           <xsd:documentation>The date when the document becomes due for payment.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="ValueDate" type="com:typeDateB">         <xsd:annotation>           <xsd:documentation>The date when the document will clear.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="ExtRef1" type="com:typeExtRefB">         <xsd:annotation>           <xsd:documentation>The value of External Reference 1.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="ExtRef2" type="com:typeExtRefB">         <xsd:annotation>           <xsd:documentation>The value of External Reference 2.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="ExtRef3" type="com:typeExtRefB">         <xsd:annotation>           <xsd:documentation>The value of External Reference 3.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="ExtRef4" type="com:typeExtRefB">         <xsd:annotation>           <xsd:documentation>The value of External Reference 4.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="ExtRef5" type="com:typeExtRefB">         <xsd:annotation>           <xsd:documentation>The value of External Reference 5.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="ExtRef6" type="com:typeExtRefB">         <xsd:annotation>           <xsd:documentation>The value of External Reference 6.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="PostData">     <xsd:annotation>       <xsd:documentation>Contains details that are used when the document is posted, or when checking that the document is postable.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element minOccurs="0" name="Template" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The code of the input template master used to input the document.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="DocumentWideData" type="inx:DocumentWideData">         <xsd:annotation>           <xsd:documentation>This element contains details that apply to all lines in the document.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="ReversingDetails" type="inx:ReversingPostData">         <xsd:annotation>           <xsd:documentation>Contains details that can be specified when posting reversing documents, or when checking that reversing documents are postable.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="RecurringDetails" type="inx:RecurringPostData">         <xsd:annotation>           <xsd:documentation>Contains details that can be specified when posting recurring documents, or when checking that recurring documents are postable.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="ControlTotal" type="com:typeMoneyB">         <xsd:annotation>           <xsd:documentation>An optional control total entered in Input.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="WorkflowCode" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The workflow process definition code.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="PositionCode" type="com:typeGeneralCode">         <xsd:annotation>           <xsd:documentation>The position code within the position hierarchy.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="ApplicationId" type="com:typeUWord">         <xsd:annotation>           <xsd:documentation>Specifies the Coda application to which this document is reserved.</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>    <xsd:complexType name="DocTotals">     <xsd:annotation>       <xsd:documentation>Contains the document totals in document, home and dual currencies.</xsd:documentation>     </xsd:annotation>     <xsd:sequence>       <xsd:element name="DocTotal" type="com:typeMoneyB">         <xsd:annotation>           <xsd:documentation>The sum of the document&apos;s debit and credit values, in document currency.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="DocCreditTotal" type="com:typeMoneyB">         <xsd:annotation>           <xsd:documentation>The sum of the document&apos;s credit values, in document currency.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="DocDebitTotal" type="com:typeMoneyB">         <xsd:annotation>           <xsd:documentation>The sum of the document&apos;s debit values, in document currency.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="HomeTotal" type="com:typeMoneyB">         <xsd:annotation>           <xsd:documentation>The sum of the document&apos;s debit and credit values, in home currency.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="HomeCreditTotal" type="com:typeMoneyB">         <xsd:annotation>           <xsd:documentation>The sum of the document&apos;s credit values, in home currency.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="HomeDebitTotal" type="com:typeMoneyB">         <xsd:annotation>           <xsd:documentation>The sum of the document&apos;s debit values, in home currency.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="DualTotal" type="com:typeMoneyB">         <xsd:annotation>           <xsd:documentation>The sum of the document&apos;s debit and credit values, in dual currency.</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element minOccurs="0" name="DualCreditTotal" type="com:typeMoneyB">         <xsd:annotation>           <xsd:documentation>The