<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:com="http://www.coda.com/efinance/schemas/common" xmlns:acm="http://www.coda.com/efinance/schemas/archivemaster" xmlns:build="http://www.coda.com/common/schemas/funcsec" targetNamespace="http://www.coda.com/efinance/schemas/archivemaster" elementFormDefault="qualified"><xsd:import namespace="http://www.coda.com/efinance/schemas/common" schemaLocation="../../../wsdl/finance/common/common.xsd"/>      <xsd:complexType name="ArchiveHeaderMasterCommon">   <xsd:sequence>    <xsd:element name="CmpCode" type="com:typeGeneralCode">     <xsd:annotation>      <xsd:documentation>The code of the company where the header master exists.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="Code" type="com:typeGeneralCode">     <xsd:annotation>      <xsd:documentation>The code of the archive header master.</xsd:documentation>     </xsd:annotation>    </xsd:element>   </xsd:sequence>  </xsd:complexType>   <xsd:complexType name="DatabaseConnection">   <xsd:annotation>    <xsd:documentation>The element contains the database settings for a Billing external archive.</xsd:documentation>   </xsd:annotation>   <xsd:sequence>    <xsd:element name="Username" type="com:typeDBConnectionField">     <xsd:annotation>      <xsd:documentation>The name of the database user, which must be entered for external archives.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="DatabaseName" type="com:typeDBConnectionField">     <xsd:annotation>      <xsd:documentation>The name of the database, which is mandatory for some databases.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="Password" type="com:typeDBConnectionFieldB">     <xsd:annotation>      <xsd:documentation>The database password, which is mandatory for some databases.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="PasswordUnset" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>If TRUE, specifies that the password for this database in not set.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="TableOwner" type="com:typeDBConnectionField">     <xsd:annotation>      <xsd:documentation>The name of the table owner, which is mandatory for all databases.</xsd:documentation>     </xsd:annotation>    </xsd:element>       </xsd:sequence>  </xsd:complexType>   <xsd:complexType name="JournalDetails">   <xsd:annotation>    <xsd:documentation>This element contains settings for the archive journal.</xsd:documentation>   </xsd:annotation>   <xsd:sequence>    <xsd:element name="NormalJournal" type="com:typeGeneralCode">     <xsd:annotation>      <xsd:documentation>The journal code used for non-turnover documents.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="TurnoverJournal" type="com:typeGeneralCode">     <xsd:annotation>      <xsd:documentation>The journal code used for turnover documents.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="Description" type="com:typeDescr">     <xsd:annotation>      <xsd:documentation>A description that appears on all journal lines.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="PauseRequestSize" type="com:typePauseRequestSize">     <xsd:annotation>      <xsd:documentation>The number of documents selected before the system checks to see if a request to pause the Archive process has been issued. The default size is 5000, and 1000 is the smallest       number allowable.      </xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element name="SuppressZeroLines" type="xsd:boolean">     <xsd:annotation>      <xsd:documentation>If TRUE specifies that lines with a line value and quantity of zero will not be stored on the archive journal.</xsd:documentation>     </xsd:annotation>    </xsd:element>   </xsd:sequence>  </xsd:complexType>   <xsd:complexType name="ProcurementDetails">   <xsd:annotation>    <xsd:documentation>If TRUE specifies that you want to run Archive for Billing.</xsd:documentation>   </xsd:annotation>   <xsd:sequence>    <xsd:element name="DisposalOfDocuments" type="com:typeCtAhmArchType">     <xsd:annotation>      <xsd:documentation>Specifies whether the documents to be archived are moved from the main Billing database into archive tables, or whether they are purged from the database.</xsd:documentation>     </xsd:annotation>    </xsd:element>    <xsd:element minOccurs="0" name="ExternalDatabase" type="acm:DatabaseConnection">     <xsd:annotation>      <xsd:documentation>The element contains the database settings for a Billing external archive.</xsd:documentation>     </xsd:annotation>    </xsd:element>   </xsd:sequence>  </xsd:complexType>     <xsd:complexType name="ArchiveHeaderMaster">   <xsd:annotation>    <xsd:documentation>This element holds an archive header master.</xsd:documentation>   </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="acm:ArchiveHeaderMasterCommon">     <xsd:sequence>      <xsd:element minOccurs="0" name="TimeStamp" type="xsd:short">       <xsd:annotation>        <xsd:documentation>The TimeStamp value for this archive header master in the database.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="Name" type="com:typeName">       <xsd:annotation>        <xsd:documentation>The full name for the archive header master.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="ShortName" type="com:typeShortName">       <xsd:annotation>        <xsd:documentation>The abbreviated name for the archive header master.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="ExternalArchive" type="xsd:boolean">       <xsd:annotation>        <xsd:documentation>If TRUE specifies that an external archive process is required for Financials data.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="JournalDetails" type="acm:JournalDetails">       <xsd:annotation>        <xsd:documentation>This element contains settings for the archive journal.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="ClusterSize" type="com:typeClusterSize">       <xsd:annotation>        <xsd:documentation>The number of documents processed into a single archive journal. This must be set to at least 10.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="DefaultSelector" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The selector master code used as the default when starting to archive documents. This field may be left blank to select all appropriate documents.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="SingleYear" type="xsd:short">       <xsd:annotation>        <xsd:documentation>Indicates whether more than one year can be added to the same archive database. To specify a single year, enter the appropriate year. To allow more than one year, leave the         field blank.        </xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="DeleteComments" type="xsd:boolean">       <xsd:annotation>        <xsd:documentation>If TRUE specifies that comments will be deleted from the Books without copying them to the archive.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="DeleteDiscounts" type="xsd:boolean">       <xsd:annotation>        <xsd:documentation>If TRUE specifies that discount information will be deleted from the Books without copying it to the archive.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="DeleteDiaryEvents" type="xsd:boolean">       <xsd:annotation>        <xsd:documentation>If TRUE specifies that diary events and comments will be deleted from the Books without copying them to the archive.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="DeleteTax" type="xsd:boolean">       <xsd:annotation>        <xsd:documentation>If TRUE specifies that tax information will be deleted from the Books without copying it to the archive. This affects any VAT contributions memos, 1099 tax memos and tax         turnover information.        </xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="DeletePay" type="xsd:boolean">       <xsd:annotation>        <xsd:documentation>If TRUE specifies that payment information will be deleted from the Books without copying it to the archive.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="ProcurementDetails" type="acm:ProcurementDetails">       <xsd:annotation>        <xsd:documentation>If TRUE specifies that you want to run Archive for Billing.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="CoreExternalDatabase" type="acm:DatabaseConnection">       <xsd:annotation>        <xsd:documentation>This element contains the database settings for a Financials external archive.</xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>                 </xsd:complexType>   <xsd:complexType name="ArchiveHeaderMasterStatus">   <xsd:annotation>    <xsd:documentation>This element holds information about the current status of the header, and the documents which have been archived using this archive header.</xsd:documentation>   </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="acm:ArchiveHeaderMasterCommon">     <xsd:sequence>      <xsd:element name="CurrentState" type="com:typeCtArchiveState">       <xsd:annotation>        <xsd:documentation>The current state of the archive header master, whether it is available, or already active.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="CurrentUser" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The user currently using this header master for an Archive process.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="FromYearsAndPeriods" type="com:typePeriod">       <xsd:annotation>        <xsd:documentation>The earliest year and period for the documents in the archive.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="ToYearsAndPeriods" type="com:typePeriod">       <xsd:annotation>        <xsd:documentation>The latest year and period for the documents in the archive.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="FromDocumentDates" type="com:typeDate">       <xsd:annotation>        <xsd:documentation>The dates of the earliest archive documents.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="ToDocumentDates" type="com:typeDate">       <xsd:annotation>        <xsd:documentation>The dates of the latest archive documents.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="FromInputDates" type="com:typeDate">       <xsd:annotation>        <xsd:documentation>The earliest input dates in the archive</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="ToInputDates" type="com:typeDate">       <xsd:annotation>        <xsd:documentation>The latest input dates in the archive.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="FromPaymentDates" type="com:typeDate">       <xsd:annotation>        <xsd:documentation>The earliest payment dates in the archive.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="ToPaymentDates" type="com:typeDate">       <xsd:annotation>        <xsd:documentation>The latest payment dates in the archive.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="ArchiveCreated" type="com:typeDate">       <xsd:annotation>        <xsd:documentation>The date and time when the first journal was created.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="LastUpdated" type="com:typeDate">       <xsd:annotation>        <xsd:documentation>The date and time when the latest journal was created.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="FirstNormalJournal" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The code of the first journal written.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="FirstNormalJournalNumber" type="com:typeDocNum">       <xsd:annotation>        <xsd:documentation>The number of the first journal written.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="FirstTurnoverJournal" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The code of the first turnover journal written.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="FirstTurnoverJournalNumber" type="com:typeDocNum">       <xsd:annotation>        <xsd:documentation>The number of the first turnover journal written.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="NumberOfDocsArchived" type="com:typeLong">       <xsd:annotation>        <xsd:documentation>The total number of documents archived.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="Paused" type="xsd:boolean">       <xsd:annotation>        <xsd:documentation>If true, indicates that the Archive process using this header is paused.</xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:element name="ArchiveHeaderMaster" type="acm:ArchiveHeaderMaster">   <xsd:annotation>    <xsd:documentation>This element holds an archive header master.</xsd:documentation>   </xsd:annotation>  </xsd:element>           <xsd:complexType name="AddArchiveHeaderMasterRequest">   <xsd:complexContent>    <xsd:extension base="com:Request">     <xsd:sequence>      <xsd:element name="ArchiveHeaderMaster" type="acm:ArchiveHeaderMaster">       <xsd:annotation>        <xsd:documentation>Contains the information for the archive header master you want to create.</xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="AddArchiveHeaderMasterRequestVerb">   <xsd:annotation>    <xsd:documentation>Creates new archive header masters in the database using the archive header information you provide.</xsd:documentation>   </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:MultiCompanyRequestVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" name="Request" type="acm:AddArchiveHeaderMasterRequest"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="AddArchiveHeaderMasterResponse">   <xsd:complexContent>    <xsd:extension base="com:MultiCompanyResponse">     <xsd:sequence>      <xsd:element name="CmpCode" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The code of the company in which you attempted to create the archive header master.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="Code" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The code of the archive header master that you attempted to create.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="TimeStamp" type="com:typeWord">       <xsd:annotation>        <xsd:documentation>The TimeStamp value for this archive header master in the database.</xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="AddArchiveHeaderMasterResponseVerb">   <xsd:annotation>    <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>   </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:ResponseVerb">     <xsd:sequence>      <xsd:element minOccurs="0" name="Companies" type="com:Companies">       <xsd:annotation>        <xsd:documentation>Contains the list of additional companies in which you want to create the masters. This is part of the multi-company functionality for maintaining masters in multiple         companies.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="acm:AddArchiveHeaderMasterResponse"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>     <xsd:complexType name="UpdateArchiveHeaderMasterRequest">   <xsd:complexContent>    <xsd:extension base="com:Request">     <xsd:sequence>      <xsd:element name="ArchiveHeaderMaster" type="acm:ArchiveHeaderMaster">       <xsd:annotation>        <xsd:documentation>Contains the information for the archive header master you want to update.</xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="UpdateArchiveHeaderMasterRequestVerb">   <xsd:annotation>    <xsd:documentation>Updates archive header masters in the database using the archive header information you provide.</xsd:documentation>   </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:MultiCompanyRequestVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" name="Request" type="acm:UpdateArchiveHeaderMasterRequest"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="UpdateArchiveHeaderMasterResponse">   <xsd:complexContent>    <xsd:extension base="com:MultiCompanyResponse">     <xsd:sequence>      <xsd:element name="CmpCode" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The code of the company in which you attempted to update the archive header master.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="Code" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The code for the archive header master that you attempted to update.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="TimeStamp" type="com:typeWord">       <xsd:annotation>        <xsd:documentation>The TimeStamp value for this archive header master in the database.</xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="UpdateArchiveHeaderMasterResponseVerb">   <xsd:annotation>    <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>   </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:ResponseVerb">     <xsd:sequence>      <xsd:element minOccurs="0" name="Companies" type="com:Companies">       <xsd:annotation>        <xsd:documentation>Contains the list of additional companies in which you want to update the masters. This is part of the multi-company functionality for maintaining masters in multiple         companies.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="acm:UpdateArchiveHeaderMasterResponse"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>     <xsd:complexType name="GetArchiveHeaderMasterRequest">   <xsd:complexContent>    <xsd:extension base="com:Request">     <xsd:sequence>      <xsd:element name="Key" type="com:Key">       <xsd:annotation>        <xsd:documentation>Contains the key identifying the archive header master you want to retrieve from the database.</xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="GetArchiveHeaderMasterRequestVerb">   <xsd:annotation>    <xsd:documentation>Retrieves the specified archive header masters from the database.</xsd:documentation>   </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:RequestVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" name="Request" type="acm:GetArchiveHeaderMasterRequest"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="GetArchiveHeaderMasterResponse">   <xsd:complexContent>    <xsd:extension base="com:Response">     <xsd:sequence>      <xsd:element name="CmpCode" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The code of the company from which you attempted to retrieve the archive header master.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="Code" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The code for the archive header master that you attempted to retrieve.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="ArchiveHeaderMaster" type="acm:ArchiveHeaderMaster">       <xsd:annotation>        <xsd:documentation>Contains the archive header master you have retrieved from the database.</xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="GetArchiveHeaderMasterResponseVerb">   <xsd:annotation>    <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>   </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:ResponseVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="acm:GetArchiveHeaderMasterResponse"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>     <xsd:complexType name="GetStatusArchiveHeaderMasterRequest">   <xsd:complexContent>    <xsd:extension base="com:Request">     <xsd:sequence>      <xsd:element name="Key" type="com:Key">       <xsd:annotation>        <xsd:documentation>Contains the key identifying the archive header master whose status you are requesting.</xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="GetStatusArchiveHeaderMasterRequestVerb">   <xsd:annotation>    <xsd:documentation>Requests information about the current status of the header, for example Idle, Active, Selecting, Posting and the documents which have been archived using this archive header     master. .</xsd:documentation>   </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:RequestVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" name="Request" type="acm:GetStatusArchiveHeaderMasterRequest"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="GetStatusArchiveHeaderMasterResponse">   <xsd:complexContent>    <xsd:extension base="com:Response">     <xsd:sequence>      <xsd:element name="CmpCode" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The code of the company in which the archive header master exists.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element name="Code" type="com:typeGeneralCode">       <xsd:annotation>        <xsd:documentation>The code of the archive header master whose status you have requested.</xsd:documentation>       </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="ArchiveHeaderMasterStatus" type="acm:ArchiveHeaderMasterStatus">       <xsd:annotation>        <xsd:documentation>Contains information about the current status of the header, for example Idle, Active, Selecting, Posting, and the documents which have been archived using this archive         header master.</xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="GetStatusArchiveHeaderMasterResponseVerb">   <xsd:annotation>    <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>   </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:ResponseVerb">     <xsd:sequence>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="acm:GetStatusArchiveHeaderMasterResponse"/>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>     <xsd:complexType name="DeleteArchiveHeaderMasterRequest">   <xsd:complexContent>    <xsd:extension base="com:Request">     <xsd:sequence>      <xsd:element name="Key" type="com:Key">       <xsd:annotation>        <xsd:documentation>Contains the key identifying the archive header master you want to delete.</xsd:documentation>       </xsd:annotation>      </xsd:element>     </xsd:sequence>    </xsd:extension>   </xsd:complexContent>  </xsd:complexType>   <xsd:complexType name="DeleteArchiveHeaderMasterRequestVerb">   <xsd:annotation>    <xsd:documentation>Deletes the specified archive header masters from the database.</xsd:documentation>   </xsd:annotation>   <xsd:complexContent>    <xsd:extension base="com:Reques