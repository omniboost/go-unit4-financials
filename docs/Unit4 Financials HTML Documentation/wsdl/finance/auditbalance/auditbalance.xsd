    <xsd:appinfo build:funcSecValue="326" build:readOnly="true"/>               <xsd:documentation>Retrieves statistical information about the progress of the audit process.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>    <xsd:complexType name="AuditBalanceResponse">     <xsd:annotation>       <xsd:documentation>This is the response that is returned when you audit balance information.</xsd:documentation>     </xsd:annotation>     <xsd:complexContent>       <xsd:extension base="com:ServiceResponse">         <xsd:sequence>           <xsd:element minOccurs="0" name="Start" type="adb:StartResponseVerb">             <xsd:annotation>               <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>             </xsd:annotation>           </xsd:element>           <xsd:element minOccurs="0" name="GetProgress" type="adb:ProgressResponseVerb">             <xsd:annotation>               <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>             </xsd:annotation>           </xsd:element>         </xsd:sequence>       </xsd:extension>     </xsd:complexContent>   </xsd:complexType>       <xsd:element name="AuditBalanceRequest" type="adb:AuditBalanceRequest">     <xsd:annotation>       <xsd:documentation>This service allows you to audit balance information.</xsd:documentation>     </xsd:annotation>   </xsd:element>   <xsd:element name="AuditBalanceResponse" type="adb:AuditBalanceResponse">     <xsd:annotation>       <xsd:documentation>This is the response that is returned when you audit balance information.</xsd:documentation>     </xsd:annotation>   </xsd:element>  </xsd:schema>