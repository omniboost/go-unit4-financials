                                    <xsd:element maxOccurs="1" minOccurs="0" name="NetValue" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The value net of tax in document currency. We always           need a value at the header level as it will           allow us to           validate that the correct line           values have been provided.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="GrossValue" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The gross value for the invoice in document currency.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="GrossHomeValue" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The gross value for the invoice in home currency.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="GrossDualValue" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The gross value for the invoice in dual currency.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="DueDate" type="com:typeHard">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Due date</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ValueDate" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Value date</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DebitNoteDueDate" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Debit note due date</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DebitNoteDueDateAmend" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Will debit note due date be amendable.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="SupplierPaymentAddress" type="pimtypes:typePimAddressNameb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The supplier payment address name</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="SupplierBank" type="pimtypes:typePimBankAccNumb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The supplier bank account number</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="SupplierPaymentAddressTag" type="com:typeWordB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The supplier payment address tag</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="SupplierBankTag" type="com:typeWordB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The supplier bank account tag</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="CompanyBank" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The company bank</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="MediaCode" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The media code</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Description" type="com:typeTextB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The line description</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Ten99Code1" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The 1099 code associated with the invoice header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Ten99Code2" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The 1099 code associated with the invoice header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Ten99Code3" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The 1099 code associated with the invoice header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Ten99Value1" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The 1099 code value with the invoice header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Ten99Value2" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The 1099 code value with the invoice header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Ten99Value3" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The 1099 code value with the invoice header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Authoriser" type="pimusr:UserKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>QUESTION FOR DESIGN: Is this the user who authorised the           document or the user who is to authorise the           document.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="SuspensePostedDocument" type="post:PostingDocument">                         <xsd:annotation>                             <xsd:documentation>                                 <p>If the invoice has been suspense posted then record the version           that was posted. This allows us to post a           reversing journal           later.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="PostingDocument" type="post:PostingDocument">                         <xsd:annotation>                             <xsd:documentation>                                 <p>This is the postable version of the document. By holding           the actual representation of what will be posted we           can allow the user to toggle between the view of the           document which they are matching against and the view of           what information will be posted to financials.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="JournalCode" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The document code that the invoice was journal posted to (i.e. 2nd stage of defer matching).</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="JournalNumber" type="com:typeDocNumAlphaB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The document number that the invoice was journal posted to (i.e. 2nd stage of defer matching).</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="ForDeferMatching" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The invoice has been marked for defer match, and is not to be picked up by standard matching.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Department" type="elmmas:ElementMasterKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The department for the invoice.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element name="PrintableCommentList" type="inv:InvoicePrintableCommentList">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The printable comment for the invoice</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="TraderCode" type="com:typeTraderCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The trader code. Only used if the supplier code is an umbrella element</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="MatchableLineCount" type="com:typeLong">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The number of matchable lines held on the           invoice.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="NumberLinesWithWriteOff" type="com:typeLongB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The number of lines on the invoice which have           write offs applied.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="NumberLinesWithDebitNote" type="com:typeLongB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The number of lines on the invoice which have           debit notes applied.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="NumberLinesWithRFC" type="com:typeLongB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The number of lines on the invoice which have           RFCs applied.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="WriteOffApplied" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Has a write off been applied to any part of this           invoice?         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="DebitNoteApplied" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Has a debit note been applied to any part of this           invoice?         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="RFCApplied" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Has an RFC been applied to any part of this           invoice?         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="SelfBillingInvoice" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Is this a Self Billing generated           invoice?         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="SelfBillingExRateDate" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The exchange rate date set by Self Billing.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="NetValueDiscrepancy" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The current net value difference on the invoice</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="InputStatus" type="pimtypes:typePimInputStatus">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The input completion status.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="DiscountPresent" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Have we got any discounts for this invoice</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscountDate1" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Settlement discount date 1</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscountDate2" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Settlement discount date 2</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscountDate3" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Settlement discount date 3</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscountDate4" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Settlement discount date 4</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscountDate5" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Settlement discount date 5</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscountValue1" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Settlement discount value 1</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscountValue2" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Settlement discount value 2</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscountValue3" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Settlement discount value 3</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscountValue4" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Settlement discount value 4</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscountValue5" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Settlement discount value 5</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscountRate1" type="com:typeNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Settlement discount rate 1</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscountRate2" type="com:typeNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Settlement discount rate 2</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscountRate3" type="com:typeNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Settlement discount rate 3</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscountRate4" type="com:typeNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Settlement discount rate 4</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscountRate5" type="com:typeNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Settlement discount rate 5</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="CancelledReason" type="com:typeDescription">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The reason why an invoice has been cancelled. Mandatory for cancelled invoices.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="CancelledDate" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The date an invoice was cancelled on. Mandatory for cancelled invoices.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element name="OrderDocumentCodeList" type="inv:InvoiceOrderDocumentCodeList">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The order document code</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="BalanceType" type="pimtypes:typePimInvoiceBalanceType">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The balanceType of the invoice is dependent upon the balanceStatus and follows a simple mapping algorithm.           By           default when an invoice has been posted from Input, its balanceType is &apos;Not Attempted&apos;.           An invoice with a           balanceStatus of Unbalanced has balanceType &apos;Not Balanced&apos;.           An invoice with a balanceStatus of Part Balanced           has balanceType &apos;Not Balanced&apos;.           An invoice with a balanceStatus of FullyBalanced has a balanceType of either           &apos;Adjusted&apos;, &apos;AdjustedByRFC&apos; or &apos;Exact&apos;           dependent upon how the balance was           achieved - was it exact or did it need           to be adjusted with the use of discrepancies?         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="BalanceStatus" type="pimtypes:typePimInvoiceBalanceState">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The balanced status of the purchase invoice. This can be Fully balanced, Not balanced, or Part balanced.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="PostStatus" type="pimtypes:typePimInvoicePostState">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The posting status of the purchase invoice. This can be Available, Unavailable, Posted, or Suspense posted.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="FailedPostCount" type="com:typeWordB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>How many times the invoice has failed to post</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="PostedDate" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The date the invoice was posted. This is the last date the invoice           was posted.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="UnassignedValue" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Returns the unassigned value of the invoice.           This is used when performing header to line           assignment. The           unassigned value is the           invoice header value minus the value of all           the lines.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="UnassignedTaxValue" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Returns the unassigned tax value of the invoice.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="UnassignedQuantity" type="com:typeNumber">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Returns the unassigned quantity of the invoice. If the invoice is fully analysed then the           unassigned quantity           is the total of all unassigned quantities on each line (if available).           If the invoice is header only then the           unassigned quantity is the total quantity on the           header minus the quantities of the generated lines.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ExplodeURL" type="com:typeText">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Transient account code - for explode URL to pass in to the authorisation</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AccrualYP" type="com:typeHPeriod">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The year/period that this invoice was accrued.           records accrual year and period when an accrual selects this           invoice and posts for it         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AccrualMasterCode" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The code of the accrual master used to accrue this invoice.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="PostableStatus" type="pimtypes:typePimInvoicePostableStatus">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The postable status of the invoice. Is the invoice ready for posting to financials?</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="MatchReference" type="com:typeULong">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The match reference of this invoice. Only set by commit when the invoice is marked as matched.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="MisMatchReasonUDFCode" type="com:typeCode">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The UDF reason code for mis-matches.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="MisMatchReasonUDFValue" type="com:typeUdfText">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The UDF mis-match reason value (non-strings converted to XML for storage).</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ValidationStatus" type="pimtypes:typePimInvoiceValidationStatus">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The validation status of the invoice.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ValidationDate" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The date that the invoice validation status was set to validation successful</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ValidationWorkflowId" type="com:typeDbGuid">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The validation workflow id relating to this invoice. This always refers           to the latest validation workflow           which may or may not have completed.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element name="ValidationWorkflowHistoryIdList" type="inv:InvoiceValidationWorkflowHistoryIdList">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The validation workflow ids relating to this invoice.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="TaxNumber" type="pimtypes:typePimVatb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The tax number from the Supplier</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DefaultSupplierAddress" type="com:typeTextAny">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The default supplier address.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="PreMatchPostableStatus" type="pimtypes:typePimInvoicePostableStatus">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The postable status of the invoice before we committed the match. We need to remember this so we can           reset the           actual postableStatus to its former value in undo matching.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ResetAndResubmitToDocumentAuthorisation" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Has this document been marked for reset and resubmit to document authorisation?</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="CompletingUser" type="pimusr:UserKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p> The completing user. </p>                                 <p> This is the user we expect to complete the workflow or when the workflow is complete, the user that actually           completed the workflow.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="OriginatingUser" type="pimusr:UserKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p> The user who originally entered the invoice. </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="CompletionWorkflowId" type="com:typeDbGuid">                         <xsd:annotation>                             <xsd:documentation>                                 <p> The completion workflow id. </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="CompletionWorkflowStatus" type="pimtypes:typePimCompletionWorkflowStatus">                         <xsd:annotation>                             <xsd:documentation>                                 <p> The completion workflow status. </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="InvoiceAdviceNoteNumberList">         <xsd:annotation>             <xsd:documentation>A list of Invoices.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="AdviceNoteNumber" type="pimtypes:typePimAdviceNoteNumberb">                 <xsd:annotation>                     <xsd:documentation>A single typePimAdviceNoteNumberb item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="InvoiceOrderNumberList">         <xsd:annotation>             <xsd:documentation>A list of Invoices.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="OrderNumber" type="pimtypes:typePimOrderNumberb">                 <xsd:annotation>                     <xsd:documentation>A single typePimOrderNumberb item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="InvoiceAuthoriseWorkflowIdList">         <xsd:annotation>             <xsd:documentation>A list of Invoices.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="AuthoriseWorkflowId" type="com:typeDbGuid">                 <xsd:annotation>                     <xsd:documentation>A single typeDbGuid item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="InvoicePrintableCommentList">         <xsd:annotation>             <xsd:documentation>A list of Invoices.</xsd:documentation>         </xsd:annotation>         <xsd:sequ