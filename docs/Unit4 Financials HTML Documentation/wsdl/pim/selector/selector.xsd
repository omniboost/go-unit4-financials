<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pimsel="http://www.coda.com/pim/schemas/selector" xmlns:com="http://www.coda.com/efinance/schemas/common" xmlns:comsel="http://www.coda.com/efinance/schemas/selector" xmlns:build="http://www.coda.com/common/schemas/funcsec" targetNamespace="http://www.coda.com/pim/schemas/selector" elementFormDefault="qualified"><xsd:import namespace="http://www.coda.com/efinance/schemas/selector" schemaLocation="../../../wsdl/finance/selector/selector.xsd"/><xsd:import namespace="http://www.coda.com/efinance/schemas/common" schemaLocation="../../../wsdl/finance/common/common.xsd"/>                              <xsd:annotation>         <xsd:documentation>Schema for the PIMSelector service.<p>This service allows you to retrieve selectors for use by Invoice Matching.</p>         </xsd:documentation>     </xsd:annotation>     <xsd:complexType name="GetRequest">         <xsd:annotation>             <xsd:documentation>Get.<p>Standard get functionality for the Selector.</p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element name="SelectorKey" type="pimsel:SelectorKey">                         <xsd:annotation>                             <xsd:documentation>The key to the Selector object.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetRequestVerb">         <xsd:annotation>             <xsd:documentation>The request of the get function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="pimsel:GetRequest">                         <xsd:annotation>                             <xsd:documentation>The details of the request for the get function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetResponse">         <xsd:annotation>             <xsd:documentation>The response of the get function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence>                     <xsd:element name="Selector" type="pimsel:Selector">                         <xsd:annotation>                             <xsd:documentation>The return value of the get function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetResponseVerb">         <xsd:annotation>             <xsd:documentation>The response verb of the get function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="pimsel:GetResponse">                         <xsd:annotation>                             <xsd:documentation>The details of the response for the get function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="PIMSelectorRequest">         <xsd:annotation>             <xsd:appinfo build:funcSecValue="832" build:readOnly="true"/>             <xsd:documentation>The PIMSelector service request.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ServiceRequest">                 <xsd:sequence>                     <xsd:element minOccurs="0" name="Get" type="pimsel:GetRequestVerb">                         <xsd:annotation>                             <xsd:appinfo build:funcSecValue="1" build:readOnly="true"/>                             <xsd:documentation>The request to the get function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="PIMSelectorResponse">         <xsd:annotation>             <xsd:documentation>The PIMSelector service response.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ServiceResponse">                 <xsd:sequence>                     <xsd:element minOccurs="0" name="Get" type="pimsel:GetResponseVerb">                         <xsd:annotation>                             <xsd:documentation>The response from the get function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>          <xsd:element name="PIMSelectorRequest" type="pimsel:PIMSelectorRequest">         <xsd:annotation>             <xsd:documentation>The Root element for the PIMSelector service request</xsd:documentation>         </xsd:annotation>     </xsd:element>     <xsd:element name="PIMSelectorResponse" type="pimsel:PIMSelectorResponse">         <xsd:annotation>             <xsd:documentation>The Root element for the PIMSelector service response</xsd:documentation>         </xsd:annotation>     </xsd:element>                               <xsd:complexType name="VocListDataElement">         <xsd:annotation>             <xsd:documentation>                 <p>A data element on the vocabulary list.</p>             </xsd:documentation>         </xsd:annotation>         <xsd:sequence>                          <xsd:element maxOccurs="1" minOccurs="0" name="Id" type="com:typeDbGuid">                 <xsd:annotation>                     <xsd:documentation>                         <p>A unique ID used internally by Coda to identify this selection vocabulary.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="0" name="Logical" type="com:typeLogicalOperator">                 <xsd:annotation>                     <xsd:documentation>                         <p>The logical connective combining this data element with the preceding data element. There is no logical           connective on the first data element.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="0" name="OpenBracket" type="com:typeLeftBrackets">                 <xsd:annotation>                     <xsd:documentation>                         <p>The number of opening parentheses used to group the data elements.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="1" name="VocabID" type="com:typeTextUpperWildB">                 <xsd:annotation>                     <xsd:documentation>                         <p>The vocabulary name of the data item that you want to select.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="1" name="OperatorID" type="com:typeVocabOpList">                 <xsd:annotation>                     <xsd:documentation>                         <p>The symbol signifying the operation to be performed on the selected vocabulary item.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="0" name="From" type="com:typeTextWild">                 <xsd:annotation>                     <xsd:documentation>                         <p>The value of the parameter or start of a range, if applicable.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="0" name="To" type="com:typeTextWild">                 <xsd:annotation>                     <xsd:documentation>                         <p>The last value in the range, if applicable.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="0" name="Modify" type="com:typeSelectorModify">                 <xsd:annotation>                     <xsd:documentation>                         <p>If TRUE specifies that editing of this parameter will be allowed at run time.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="0" name="Prompt" type="com:typeTextB">                 <xsd:annotation>                     <xsd:documentation>                         <p>The text that prompts the user for input at run time, if you have allowed modification.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="0" name="CloseBracket" type="com:typeRightBrackets">                 <xsd:annotation>                     <xsd:documentation>                         <p>The number of closing parentheses used to group the data elements.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="VocListDataElementList">         <xsd:annotation>             <xsd:documentation>A list of VocListDataElements.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="VocListDataElement" type="pimsel:VocListDataElement">                 <xsd:annotation>                     <xsd:documentation>A single VocListDataElement item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="VocListDataElementKeyList">         <xsd:annotation>             <xsd:documentation>A list of keys for VocListDataElement items.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="VocListDataElement" type="pimsel:VocListDataElementKey">                 <xsd:annotation>                     <xsd:documentation>A single key for a VocListDataElement item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="VocListDataElementKey">         <xsd:annotation>             <xsd:documentation>A single key for a VocListDataElement item.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element minOccurs="0" name="CompanyCode" type="com:typeCode">                 <xsd:annotation>                     <xsd:documentation>                         <p>The company code.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>             <xsd:element minOccurs="0" name="MasterCode" type="com:typeCode">                 <xsd:annotation>                     <xsd:documentation>                         <p>The selector code.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>             <xsd:element minOccurs="0" name="Id" type="com:typeDbGuid">                 <xsd:annotation>                     <xsd:documentation>                         <p>A unique ID used internally by Coda to identify this selection vocabulary.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>          <xsd:complexType name="SelectorType">         <xsd:annotation>             <xsd:documentation>                 <p>Indicates the type of selector: the data class you are using, and the source of the data, retrieved from Books,         Intray, Archive or Self.</p>             </xsd:documentation>         </xsd:annotation>         <xsd:sequence>                          <xsd:element maxOccurs="1" minOccurs="1" name="DataClass" type="com:typeCode">                 <xsd:annotation>                     <xsd:documentation>                         <p>The dataclass used by the selector.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="0" name="Myself" type="com:typeBoolean">                 <xsd:annotation>                     <xsd:documentation>                         <p>Data located on the same tables as the selected data class will be retrieved.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>                          <xsd:element maxOccurs="1" minOccurs="0" name="Books" type="com:typeBoolean">                 <xsd:annotation>                     <xsd:documentation>                         <p>Data located in the Books will be retrieved.</p>                     </xsd:documentation>                 </xsd:annotation>             </xsd:element>           