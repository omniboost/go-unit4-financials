<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:lrn="http://www.coda.com/pim/schemas/logicalreceiptnote" xmlns:procsys="http://www.coda.com/pim/schemas/procsystem" xmlns:elmmas="http://www.coda.com/pim/schemas/elementmaster" xmlns:com="http://www.coda.com/efinance/schemas/common" xmlns:pimusr="http://www.coda.com/pim/schemas/user" xmlns:inv="http://www.coda.com/pim/schemas/invoice" xmlns:cmt="http://www.coda.com/pim/schemas/comment" xmlns:pimtypes="http://www.coda.com/pim/schemas/common" xmlns:doc="http://www.coda.com/pim/schemas/document" xmlns:group="http://www.coda.com/pim/schemas/group" xmlns:build="http://www.coda.com/common/schemas/funcsec" xmlns:datatypes="http://www.coda.com/common/schemas/datatypes" xmlns:curr="http://www.coda.com/pim/schemas/currency" targetNamespace="http://www.coda.com/pim/schemas/logicalreceiptnote" elementFormDefault="qualified"><xsd:import namespace="http://www.coda.com/common/schemas/datatypes" schemaLocation="../../../wsdl/common/datatypes/datatypes.xsd"/><xsd:import namespace="http://www.coda.com/pim/schemas/common" schemaLocation="../../../wsdl/pim/common/common.xsd"/><xsd:import namespace="http://www.coda.com/pim/schemas/invoice" schemaLocation="../../../wsdl/pim/invoice/invoice.xsd"/><xsd:import namespace="http://www.coda.com/pim/schemas/group" schemaLocation="../../../wsdl/pim/group/group.xsd"/><xsd:import namespace="http://www.coda.com/pim/schemas/elementmaster" schemaLocation="../../../wsdl/pim/elementmaster/elementmaster.xsd"/><xsd:import namespace="http://www.coda.com/pim/schemas/user" schemaLocation="../../../wsdl/pim/user/user.xsd"/><xsd:import namespace="http://www.coda.com/pim/schemas/comment" schemaLocation="../../../wsdl/pim/comment/comment.xsd"/><xsd:import namespace="http://www.coda.com/efinance/schemas/common" schemaLocation="../../../wsdl/finance/common/common.xsd"/><xsd:import namespace="http://www.coda.com/pim/schemas/procsystem" schemaLocation="../../../wsdl/pim/procsystem/procsystem.xsd"/><xsd:i