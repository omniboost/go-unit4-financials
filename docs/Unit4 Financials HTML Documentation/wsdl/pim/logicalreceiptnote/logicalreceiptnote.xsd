etResponse">         <xsd:annotation>             <xsd:documentation>The response of the get function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence>                     <xsd:element name="LRN" type="lrn:LRN">                         <xsd:annotation>                             <xsd:documentation>The return value of the get function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="GetResponseVerb">         <xsd:annotation>             <xsd:documentation>The response verb of the get function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="lrn:GetResponse">                         <xsd:annotation>                             <xsd:documentation>The details of the response for the get function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="FindRequest">         <xsd:annotation>             <xsd:documentation>Find.<p>Standard find functionality for the logical receipt note.</p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element name="Query" type="com:Query">                         <xsd:annotation>                             <xsd:documentation>The set of query criteria to be used to find the LRN objects that the user is interested in.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="FindRequestVerb">         <xsd:annotation>             <xsd:documentation>The request of the find function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="lrn:FindRequest">                         <xsd:annotation>                             <xsd:documentation>The details of the request for the find function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="FindResponse">         <xsd:annotation>             <xsd:documentation>The response of the find function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence>                     <xsd:element name="LRNList" type="lrn:LRNList">                         <xsd:annotation>                             <xsd:documentation>The return value of the find function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="FindResponseVerb">         <xsd:annotation>             <xsd:documentation>The response verb of the find function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="lrn:FindResponse">                         <xsd:annotation>                             <xsd:documentation>The details of the response for the find function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="DeleteRequest">         <xsd:annotation>             <xsd:documentation>Delete.<p>Standard delete functionality for the logical receipt note.</p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element name="LRNKey" type="lrn:LRNKey">                         <xsd:annotation>                             <xsd:documentation>The key to the LRN object.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="DeleteRequestVerb">         <xsd:annotation>             <xsd:documentation>The request of the delete function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="lrn:DeleteRequest">                         <xsd:annotation>                             <xsd:documentation>The details of the request for the delete function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="DeleteResponse">         <xsd:annotation>             <xsd:documentation>The response of the delete function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence/>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="DeleteResponseVerb">         <xsd:annotation>             <xsd:documentation>The response verb of the delete function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="lrn:DeleteResponse">                         <xsd:annotation>                             <xsd:documentation>The details of the response for the delete function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="DeleteLineRequest">         <xsd:annotation>             <xsd:documentation>DeleteLine.<p>Standard delete functionality for the logical receipt note line.</p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element name="LRNLinesList" type="lrn:LRNLineList">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The list of LRN lines that you want to delete.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="DeleteLineRequestVerb">         <xsd:annotation>             <xsd:documentation>The request of the deleteLine function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="lrn:DeleteLineRequest">                         <xsd:annotation>                             <xsd:documentation>The details of the request for the deleteLine function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="DeleteLineResponse">         <xsd:annotation>             <xsd:documentation>The response of the deleteLine function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence/>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="DeleteLineResponseVerb">         <xsd:annotation>             <xsd:documentation>The response verb of the deleteLine function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="lrn:DeleteLineResponse">                         <xsd:annotation>                             <xsd:documentation>The details of the response for the deleteLine function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="AddRequest">         <xsd:annotation>             <xsd:documentation>Add.<p>Standard add functionality for the logical receipt note.</p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element name="LRN" type="lrn:LRN">                         <xsd:annotation>                             <xsd:documentation>The whole LRN object that is to be persisted in the system.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="AddRequestVerb">         <xsd:annotation>             <xsd:documentation>The request of the add function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="lrn:AddRequest">                         <xsd:annotation>                             <xsd:documentation>The details of the request for the add function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="AddResponse">         <xsd:annotation>             <xsd:documentation>The response of the add function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence>                     <xsd:element name="LRN" type="lrn:LRN">                         <xsd:annotation>                             <xsd:documentation>The return value of the add function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="AddResponseVerb">         <xsd:annotation>             <xsd:documentation>The response verb of the add function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="lrn:AddResponse">                         <xsd:annotation>                             <xsd:documentation>The details of the response for the add function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="UpdateRequest">         <xsd:annotation>             <xsd:documentation>Update.<p>Standard update functionality for the logical receipt note.</p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Request">                 <xsd:sequence>                     <xsd:element name="LRN" type="lrn:LRN">                         <xsd:annotation>                             <xsd:documentation>The whole LRN object that is to be persisted in the system.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="UpdateRequestVerb">         <xsd:annotation>             <xsd:documentation>The request of the update function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:RequestVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Request" type="lrn:UpdateRequest">                         <xsd:annotation>                             <xsd:documentation>The details of the request for the update function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="UpdateResponse">         <xsd:annotation>             <xsd:documentation>The response of the update function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:Response">                 <xsd:sequence>                     <xsd:element name="LRN" type="lrn:LRN">                         <xsd:annotation>                             <xsd:documentation>The return value of the update function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="UpdateResponseVerb">         <xsd:annotation>             <xsd:documentation>The response verb of the update function.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ResponseVerb">                 <xsd:sequence>                     <xsd:element maxOccurs="unbounded" minOccurs="0" name="Response" type="lrn:UpdateResponse">                         <xsd:annotation>                             <xsd:documentation>The details of the response for the update function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="PIMLogicalReceiptNoteRequest">         <xsd:annotation>             <xsd:appinfo build:readOnly="true"/>             <xsd:documentation>The PIMLogicalReceiptNote service request.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ServiceRequest">                 <xsd:sequence>                     <xsd:element minOccurs="0" name="Get" type="lrn:GetRequestVerb">                         <xsd:annotation>                             <xsd:appinfo build:readOnly="true"/>                             <xsd:documentation>The request to the get function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element minOccurs="0" name="Find" type="lrn:FindRequestVerb">                         <xsd:annotation>                             <xsd:appinfo build:readOnly="true"/>                             <xsd:documentation>The request to the find function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element minOccurs="0" name="Delete" type="lrn:DeleteRequestVerb">                         <xsd:annotation>                             <xsd:appinfo build:readOnly="false"/>                             <xsd:documentation>The request to the delete function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element minOccurs="0" name="DeleteLine" type="lrn:DeleteLineRequestVerb">                         <xsd:annotation>                             <xsd:appinfo build:readOnly="false"/>                             <xsd:documentation>The request to the deleteLine function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element minOccurs="0" name="Add" type="lrn:AddRequestVerb">                         <xsd:annotation>                             <xsd:appinfo build:readOnly="false"/>                             <xsd:documentation>The request to the add function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element minOccurs="0" name="Update" type="lrn:UpdateRequestVerb">                         <xsd:annotation>                             <xsd:appinfo build:readOnly="false"/>                             <xsd:documentation>The request to the update function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="PIMLogicalReceiptNoteResponse">         <xsd:annotation>             <xsd:documentation>The PIMLogicalReceiptNote service response.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="com:ServiceResponse">                 <xsd:sequence>                     <xsd:element minOccurs="0" name="Get" type="lrn:GetResponseVerb">                         <xsd:annotation>                             <xsd:documentation>The response from the get function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element minOccurs="0" name="Find" type="lrn:FindResponseVerb">                         <xsd:annotation>                             <xsd:documentation>The response from the find function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element minOccurs="0" name="Delete" type="lrn:DeleteResponseVerb">                         <xsd:annotation>                             <xsd:documentation>The response from the delete function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element minOccurs="0" name="DeleteLine" type="lrn:DeleteLineResponseVerb">                         <xsd:annotation>                             <xsd:documentation>The response from the deleteLine function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element minOccurs="0" name="Add" type="lrn:AddResponseVerb">                         <xsd:annotation>                             <xsd:documentation>The response from the add function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element minOccurs="0" name="Update" type="lrn:UpdateResponseVerb">                         <xsd:annotation>                             <xsd:documentation>The response from the update function.</xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>          <xsd:element name="PIMLogicalReceiptNoteRequest" type="lrn:PIMLogicalReceiptNoteRequest">         <xsd:annotation>             <xsd:documentation>The Root element for the PIMLogicalReceiptNote service request</xsd:documentation>         </xsd:annotation>     </xsd:element>     <xsd:element name="PIMLogicalReceiptNoteResponse" type="lrn:PIMLogicalReceiptNoteResponse">         <xsd:annotation>             <xsd:documentation>The Root element for the PIMLogicalReceiptNote service response</xsd:documentation>         </xsd:annotation>     </xsd:element>                                                                                 <xsd:complexType name="LRN">         <xsd:annotation>             <xsd:documentation>                 <p>The logical receipt note.</p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="doc:Document">                 <xsd:sequence>                                          <xsd:element maxOccurs="1" minOccurs="1" name="DocumentCode" type="com:typeCode">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The document code of the lrn in it&apos;s source system. We just           store the code as provided to us and have no way           of validating           it is a proper document code. For POP it should be one of their           document codes, for another           procurement system it could just be           a piece of text.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="DocumentNumber" type="pimtypes:typePimLrnDocnum">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The number of the lrn document. If the document has come from           a procurement system (including POP) then this           field should be           populated for us. If the document is a system generated document,           i.e. as a RFC, then we will           need to create this number ourselves.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="TraderCode" type="com:typeTraderCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The trader code. Only used if the supplier code is an umbrella element</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="NetValue" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The value net of tax in document currency.           Always need a value at the header level as it will be used to           validate that the correct line           values have been provided.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="GrossValue" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The value gross of tax in document currency.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="GrossHomeValue" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The value gross of tax in home currency.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="GrossDualValue" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The value gross of tax in dual currency.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="TaxValue" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The tax value in document currency.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="TaxHomeValue" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The tax value in home currency.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="TaxDualValue" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The tax value in dual currency.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AdviceNoteNumber" type="pimtypes:typePimAdviceNoteNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The advice note number relating to the header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="DeliveryDate" type="com:typeHard">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The date this LRN was delivered.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderOrderNumber" type="pimtypes:typePimOrderNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Derived from order details linked to LRN Lines. Where more than one order number then header value is null.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Invoice" type="inv:InvoiceKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Only applicable for RFC. The invoice this LRN relates to</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="SupplierInvoiceNumber" type="pimtypes:typePimSupplierInvoiceNumber">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Only applicable for RFC. This can be derived from the invoice           the RFC is linked to.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element name="LineList" type="lrn:LRNLineList">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The LRNs lines.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ExternalSystem" type="procsys:ProcurementSystemMasterKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The external procurement system from where the           LRN came from. The system will validate that           the user extension           master has the correct           usage, i.e. the external procurement system one.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ExternalSystemDocumentKey" type="com:typeTextB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Field to record the key of the header in the           external procurement system. PIM has no idea           what procurement           system this LRN has come from.           Therefore it is upto the external system to make           sure that it can uniquely           identify where this header           came from based upon the value held by this property.           PIM will simply store whatever           value is passed to it           and use this when calling back to the external system.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ReceiptStatus" type="pimtypes:typePimLrnReceiptStatus">                         <xsd:annotation>                             <xsd:documentation>                                 <p>What stage is the LRN at, first or second stage? If the           procurement system doesn&apos;t have two stage receiving           set           then it should set it directly to the second stage and           set the second stage date to the date it was           received.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="SecondStageDate" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>When the LRN goes to the second stage this should           record the date that happened.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="MatchedStatus" type="pimtypes:typePimLrnMatchedStatus">                         <xsd:annotation>                             <xsd:documentation>                                 <p>           The matched status of the LRN. The possible values are:           <ul>                                         <li/>             Not matched             <li/>             Part matched             <li/>             Fully matched           </ul>                                 </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="TermsCode" type="com:typeTermB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The terms code from the order</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element name="PrintableCommentList" type="lrn:LRNPrintableCommentList">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The printable comment for the LRN header. At 11.2 this is only used to hold printable comments on RFC           headers.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="LRNPrintableCommentList">         <xsd:annotation>             <xsd:documentation>A list of LRNs.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="PrintableComment" type="com:typeTextB">                 <xsd:annotation>                     <xsd:documentation>A single typeTextB item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="LRNList">         <xsd:annotation>             <xsd:documentation>A list of LRNs.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="LRN" type="lrn:LRN">                 <xsd:annotation>                     <xsd:documentation>A single LRN item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="LRNKeyList">         <xsd:annotation>             <xsd:documentation>A list of keys for LRN items.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="LRN" type="lrn:LRNKey">                 <xsd:annotation>                     <xsd:documentation>A single key for a LRN item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="LRNKey">         <xsd:annotation>             <xsd:documentation>A single key for a LRN item.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="doc:DocumentKey">                 <xsd:sequence/>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>          <xsd:complexType name="LRNLine">         <xsd:annotation>             <xsd:documentation>                 <p>The LRN Line.</p>             </xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="doc:Line">                 <xsd:sequence>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderAdviceNoteNumber" type="pimtypes:typePimAdviceNoteNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The advice note number held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderAttachmentId" type="com:typeDbGuid">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The attachment ID held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="HeaderCompanyCode" type="com:typeCode">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The company code held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="HeaderDeliveryDate" type="com:typeHard">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The delivery date held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="HeaderDocumentCode" type="com:typeCode">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The document code held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="HeaderDocumentID" type="com:typeDbGuid">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The document ID held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="HeaderDocumentNumber" type="pimtypes:typePimLrnDocnum">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The document number held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="HeaderDocumentType" type="pimtypes:typePimDocumentType">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The document type held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderContractNumber" type="com:typeTextB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The contract number held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfDate1" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The user-defined field Date 1 held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfDate2" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The user-defined field Date 2 held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfDate3" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The user-defined field Date 3 held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfDate4" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The user-defined field Date 4 held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfDate5" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The user-defined field Date 5 held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfDate6" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The user-defined field Date 6 held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfElement1" type="com:typeElmCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The user-defined field Element 1 held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfElement2" type="com:typeElmCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The user-defined field Element 2 held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfElement3" type="com:typeElmCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The user-defined field Element 3 held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfElement4" type="com:typeElmCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The user-defined field Element 4 held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfElement5" type="com:typeElmCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The user-defined field Element 5 held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfElement6" type="com:typeElmCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The user-defined field Element 6 held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfNumber1" type="com:typeNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>A user-defined field Number held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfNumber2" type="com:typeNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>A user-defined field Number held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfNumber3" type="com:typeNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>A user-defined field Number held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfNumber4" type="com:typeNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>A user-defined field Number held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfNumber5" type="com:typeNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>A user-defined field Number held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfNumber6" type="com:typeNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>A user-defined field Number held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfText1" type="com:typeTextB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>A user-defined field Text held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfText2" type="com:typeTextB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>A user-defined field Text held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfText3" type="com:typeTextB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>A user-defined field Text held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfText4" type="com:typeTextB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>A user-defined field Text held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfText5" type="com:typeTextB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>A user-defined field Text held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderUdfText6" type="com:typeTextB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>A user-defined field Text held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderDualCurrencyCode" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The dual currency code held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                     <xsd:element name="HeaderPrintCommentList" type="lrn:LRNLineHeaderPrintCommentList">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The list of printable comments held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="HeaderHomeCurrencyCode" type="com:typeCode">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The home currency code held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="HeaderInputDate" type="com:typeHard">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The input date held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderInvoiceDocumentID" type="com:typeDbGuid">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The invoice document ID held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderMatchedDate" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The matched date held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderReceiptStatus" type="pimtypes:typePimLrnReceiptStatus">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The receipt status held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderSecondStageDate" type="com:typeHardb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The second stage date held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="HeaderSupplierLevel" type="com:typeElmLevel">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The supplier level held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderSupplierInvoiceNumber" type="pimtypes:typePimSupplierInvoiceNumber">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The supplier invoice number held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="HeaderSupplierCode" type="com:typeCode">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The supplier code held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="HeaderSupplierName" type="com:typeName">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The supplier name held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="HeaderSupplierShortName" type="com:typeShortName">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The supplier short name held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="HeaderTraderCode" type="com:typeTraderCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The trader code held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="HeaderUserName" type="com:typeName">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The user name held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="HeaderYearPeriod" type="com:typeHPeriod">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The year and period held on the LRN header.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="OrderDocumentID" type="com:typeTextB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Concatenation of order document code and order number - this is purely here so we have a property to map the           vocab ID to on the client.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ContractNumber" type="com:typeTextB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The contract number this line relates to.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Department" type="elmmas:ElementMasterKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The department for the lrn line.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="OrderDocumentCode" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The order document code that this LRN line relates to.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="OrderNumber" type="pimtypes:typePimOrderNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The order that this LRN line relates to.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="OrderDate" type="com:typeHard">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The date of the order that this LRN line relates to.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element name="RequisitionerKeyList" type="pimusr:UserKeyList">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The requisitioner(s) for the order line(s) associated with this LRN line.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="QuantityReceived" type="com:typeNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The quantity of received items this line is for.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="SecondPrice" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The second price for the item received.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="DocumentRate" type="datatypes:Rate">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The effective rate on the document currency.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="HomeRate" type="datatypes:Rate">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The effective rate on the home currency.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DualRate" type="datatypes:Rate">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The effective rate on the dual currency.           Only present if company is configured with a dual currency.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ParentRate" type="datatypes:Rate">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The effective rate on the parent currency.           Only present if required in order to convert between document, home           and dual.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="ValueReceived" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>This is the value received in document currency. It is supplied by the external           procurement system.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="HomeValueReceived" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>This is the value received in home value. This will be supplied by the external           procurement system.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DualValueReceived" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>This is the value received in dual value. This will be supplied by the external           procurement system if           appropriate.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="SplitLine" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Determines whether the LRN line has been split. If it has more than           one MLine hanging off it then it has.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Journal" type="com:typeBoolean">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Has a journal been created for this line? This will be true           if it has had LRN housekeeping run on it.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Account" type="doc:Account">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The account associated with this LRN line.           It is not mandatory as it isn&apos;t a required           field within POP,           therefore there might           not be one!         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Comments" type="com:typeTextB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Any comments held against the line.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="Authoriser" type="pimusr:UserKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>QUESTION FOR DESIGN: Presumably this is the user who           authorised the line in the other system? Do we need           to           know this or do the lines need to be authorised in           PIM before they can be matched?         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ExternalSystemLineKey" type="com:typeText">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Field to record the key of the item line in the           external procurement system. PIM has no idea           what procurement           system this LRN has come from.           Therefore it is upto the external system to make           sure that it can uniquely           identify where this line           came from based upon the value held by this property.           PIM will simply store whatever           value is passed to it           and use this when calling back to the external system.         </p>                                 <p>This field is non-mandatory because if we create an RFC it           is an internal document and as such will have no           external           line key.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="UnmatchedQuantity" type="com:typeNumber">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The unmatched quantity on this LRN line. If the           line is value only then this property should           be set to null.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="UnmatchedValue" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The unmatched value on this LRN line. If the           line is value only then it is calculated           from the difference           between the value of the           line and the sum of the mLines that are linked to           matched invoices. If it has           quantity           and price then it is calculated from           the unmatched quantity multiplied by the unit           price.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="MatchingCurrency" type="curr:CurrencyKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>THIS PROPERTY IS FOR INTERNAL USE ONLY AND IS NEVER PERSISTED TO THE DATABASE, ALTHOUGH WE           DO WANT A DATABASE           COLUMN SO WE CAN HAVE A METADATA VOCAB FOR IT.            Within assign and balance in interactive matching, this property           is used soley by the client to allow the user to see the           currency of the invoice to which this LRN is being           assigned.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="MatchingCurrencyValue" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>THIS PROPERTY IS FOR INTERNAL USE ONLY AND IS NEVER PERSISTED TO THE DATABASE, ALTHOUGH WE           DO WANT A DATABASE           COLUMN SO WE CAN HAVE A METADATA VOCAB FOR IT.            Within assign and balance in interactive matching, this property           is used solely by the client to copy the converted           currency value from an MLine into the LRNLine, so that the           user can see lrn values in invoice currency.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="MatchedStatus" type="pimtypes:typePimLrnLineMatchedStatus">                         <xsd:annotation>                             <xsd:documentation>                                 <p>           The matched status of the lrn line. Possible values           are:            <ul>                                         <li/>             Not matched - no part of the lrn line has been matched             <li/>             Part matched - some but not all of the lrn line has been matched             <li/>             Fully matched - all of the lrn line has been matched           </ul>                                 </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="WIPStatus" type="pimtypes:typePimLrnLineWipStatus">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Status to indicate whether the lrn line is in a wip group.           If it is in 1 single group then the status will           indicate the reason           in the same way the groups wip status does. If it is in multiple wip           groups it will use a           status akin to #wip.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element name="LineCommentsList" type="cmt:CommentList">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Any comments held against the LRN line.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="UnassignedValue" type="com:Money">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Returns the value of the lrn line yet to be assigned.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="UnassignedQuantity" type="com:typeNumberb">                         <xsd:annotation>                             <xsd:documentation>                                 <p>Returns the quantity of the lrn line yet to be assigned.           This will be null if it is a value only line.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="DocumentCode" type="com:typeCode">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The document code on the LRN.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="1" name="DocumentNumber" type="pimtypes:typePimLrnDocnum">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The document number on the LRN.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AccrualYP" type="com:typeHPeriod">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The year/period that this LRN was accrued.           record accrual year and period when an accrual selects this LRN           line and posts for it.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="AccrualMasterCode" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The code of the accrual master used to accrue this invoice.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ContractReference" type="com:typeDescription">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The contract reference from POP</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="OrderRaiser" type="pimusr:UserKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The User who raised the order</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="OrderReceiver" type="pimusr:UserKey">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The User who received the Order</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ProductGroup" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The Product group - hierarchy level 1 above item</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="ProductGroup2" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The Product group - hierarchy level 2 above item</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscrepancyReasonCode" type="com:typeCodeB">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The matching master code of the reason for the discrepancy.</p>                                 <p>We need the three constituent parts of the discrepancy reason key because           we can&apos;t refer to it via           aggregation. This is because it is owned by the           matching master.         </p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                                          <xsd:element maxOccurs="1" minOccurs="0" name="DiscrepancyReasonDescription" type="com:typeDescription">                         <xsd:annotation>                             <xsd:documentation>                                 <p>The description for the discrepancy.</p>                             </xsd:documentation>                         </xsd:annotation>                     </xsd:element>                 </xsd:sequence>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>     <xsd:complexType name="LRNLineHeaderPrintCommentList">         <xsd:annotation>             <xsd:documentation>A list of LRNLines.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="HeaderPrintComment" type="com:typeTextB">                 <xsd:annotation>                     <xsd:documentation>A single typeTextB item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="LRNLineList">         <xsd:annotation>             <xsd:documentation>A list of LRNLines.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="LRNLine" type="lrn:LRNLine">                 <xsd:annotation>                     <xsd:documentation>A single LRNLine item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="LRNLineKeyList">         <xsd:annotation>             <xsd:documentation>A list of keys for LRNLine items.</xsd:documentation>         </xsd:annotation>         <xsd:sequence>             <xsd:element maxOccurs="unbounded" minOccurs="0" name="LRNLine" type="lrn:LRNLineKey">                 <xsd:annotation>                     <xsd:documentation>A single key for a LRNLine item.</xsd:documentation>                 </xsd:annotation>             </xsd:element>         </xsd:sequence>     </xsd:complexType>     <xsd:complexType name="LRNLineKey">         <xsd:annotation>             <xsd:documentation>A single key for a LRNLine item.</xsd:documentation>         </xsd:annotation>         <xsd:complexContent>             <xsd:extension base="doc:LineKey">                 <xsd:sequence/>             </xsd:extension>         </xsd:complexContent>     </xsd:complexType>      </xsd:schema>